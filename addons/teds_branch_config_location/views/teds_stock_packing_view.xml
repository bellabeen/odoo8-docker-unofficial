<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
	 	<record id="view_teds_stock_packing_tipe_source_form" model="ir.ui.view">
            <field name="name">teds.stock.packing.tipe.source.form</field>
            <field name="model">wtc.stock.packing</field>
            <field name="inherit_id" ref="wtc_stock.wtc_stock_packing_out_ws_umum_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//form/sheet/group[last()]" position="after">
                    <group coslpan="4" col="4">
                            <field name="tipe_source" attrs="{'invisible':[('tipe_source','=',False)]}"/>
                    </group>
                </xpath>
            </field>
        </record>

        <record id="view_teds_stock_packing_tipe_source_po_form" model="ir.ui.view">
            <field name="name">teds.stock.packing.tipe.source.po.form</field>
            <field name="model">wtc.stock.packing</field>
            <field name="inherit_id" ref="wtc_stock.wtc_stock_packing_in_ws_umum_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//form/sheet/group[last()]" position="after">
                	<group coslpan="4" col="4">
                        <field name="tipe_source" attrs="{'invisible':[('tipe_source','=',False)]}"/>
                    </group>
                </xpath>
            </field>
        </record>

    </data>
</openerp>