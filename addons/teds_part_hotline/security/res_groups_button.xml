<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

<!-- Reset PO -->
        <record id="group_button_reset_po_part_hotline" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Reset PO Part Hotline</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_part_hotline_read'))]"/>
        </record>

<!-- Reset WO -->
        <record id="group_button_reset_wo_part_hotline" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Reset WO Part Hotline</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_part_hotline_read'))]"/>
        </record>

<!-- Check Available -->
        <record id="group_button_check_available_part_hotline" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Check Available Part Hotline</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_part_hotline_read'))]"/>
        </record>
        
        <record id="access_button_check_available_part_hotline_read" model="ir.model.access">
            <field name="model_id" ref="model_teds_part_hotline"/>
            <field name="name">button_check_available_part_hotline read</field>
            <field name="group_id" ref="group_button_check_available_part_hotline"/>
            <field name="perm_write">1</field>
        </record>

<!-- RFA -->
        <record id="group_button_rfa_part_hotline" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">RFA Part Hotline</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_part_hotline_read'))]"/>
        </record>
        
        <record id="access_button_rfa_part_hotline_read" model="ir.model.access">
            <field name="model_id" ref="model_teds_part_hotline"/>
            <field name="name">button_rfa_part_hotline read</field>
            <field name="group_id" ref="group_button_rfa_part_hotline"/>
            <field name="perm_write">1</field>
        </record>

<!-- Change Product -->
        <record id="group_button_change_product_part_hotline" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Change Product Part Hotline</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_part_hotline_read'))]"/>
        </record>

<!-- Check Available -->
        <record id="group_button_approved_part_hotline" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Approved Part Hotline</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_part_hotline_read'))]"/>
        </record>
        
        <record id="access_button_approved_part_hotline_read" model="ir.model.access">
            <field name="model_id" ref="model_teds_part_hotline"/>
            <field name="name">button_approve_part_hotline read</field>
            <field name="group_id" ref="group_button_approved_part_hotline"/>
            <field name="perm_write">1</field>
        </record>

        <record id="wtc_branch.group_sys_admin" model="res.groups">
            <field name="implied_ids" eval="[
                (4,ref('group_button_check_available_part_hotline')),
                (4,ref('group_button_rfa_part_hotline')),
                (4,ref('group_button_approved_part_hotline')),
                (4,ref('group_button_change_product_part_hotline')),
                (4,ref('group_button_reset_po_part_hotline')),
                (4,ref('group_button_reset_wo_part_hotline')),
                
            ]"/> 
            </record> 
    </data>
</openerp>