<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data> 
		<record model="ir.ui.view" id="wtc_branch_config_purchase_asset_view">
		    <field name="name">wtc.branch.config.purchase.assset.form.view</field>
		    <field name="model">wtc.branch.config</field>
		    <field name="inherit_id" ref="wtc_branch.wtc_branch_config_form_view"/>
		    <field name="arch" type="xml">	
                <xpath expr="/form/sheet/group" position="after">                  
					<group string="Purchase Asset">
						<field name="purchase_asset_journal_id"/>
						<field name="receipt_asset_journal_id"/>	
						<field name="journal_asset_adjustment_id"/>	
						<field name="journal_disposal_asset_id"/>														
						<field name='prepaid_account_id'/>
						<field name='accrue_account_id'/>	
						<field name='gain_loss_account_id'/>
						<field name='expense_asset_account_id'/>
						<field name='journal_disposal_asset_hl_id'/>
					</group>                  
                </xpath>				
		    </field>
		</record>
	</data>
</openerp>
															