<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
    
<!-- Report Asset -->
		
        <record id="group_wtc_report_asset_wizard_read" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Report Asset/Prepaid Read</field>
            <field name="menu_access" eval="[
            								(4, ref('menu_asset_report')),
            								]"/>
        </record>

        <record id="group_wtc_report_asset_wizard_update" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Report Asset/Prepaid Update</field>
            <field name="menu_access" eval="[
            								(4, ref('menu_asset_report')),
            								]"/>
			<field name="implied_ids" eval="[(4, ref('group_wtc_report_asset_wizard_read'))]"/>
        </record>
        
        <record id="wtc_branch.group_sys_admin" model="res.groups">
			<field name="implied_ids" eval="[(4,ref('group_wtc_report_asset_wizard_read')),
											(4,ref('group_wtc_report_asset_wizard_update')),
											]"/>
		</record>
    </data>
</openerp>
