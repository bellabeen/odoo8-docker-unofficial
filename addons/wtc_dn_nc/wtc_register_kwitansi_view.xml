<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
    	
	<record id="view_wtc_register_kwitansi_line_new_tree_dn" model="ir.ui.view">
        <field name="name">wtc.register.kwitansi.line.new.tree.dn</field>
        <field name="model">wtc.register.kwitansi.line</field>
        <field name="inherit_id" ref="wtc_register_kwitansi.view_register_kwitansi_tree"/>
        <field name="arch" type="xml">
        	<field name="payment_id" position='after'>
        		<field name='dn_nc_id' options="{'no_open':True,'no_create_edit':True}"/>                  	                    	
        	</field>
        </field>
    </record> 
            
	<record id="view_wtc_register_kwitansi_line_new_form_dn" model="ir.ui.view">
        <field name="name">wtc.register.kwitansi.line.new.form.dn</field>
        <field name="model">wtc.register.kwitansi.line</field>
        <field name="inherit_id" ref="wtc_register_kwitansi.view_register_kwitansi_form"/>
        <field name="arch" type="xml">
        	<field name="payment_id" position='after'>
        		<field name='dn_nc_id' options="{'no_open':True,'no_create_edit':True}"/>                  	                    	
        	</field>
        </field>
    </record> 
    
	<record id="view_wtc_register_kwitansi_line_new_search_dn" model="ir.ui.view">
        <field name="name">wtc.register.kwitansi.line.new.search.dn</field>
        <field name="model">wtc.register.kwitansi.line</field>
        <field name="inherit_id" ref="wtc_register_kwitansi.view_register_kwitansi_line_search"/>
        <field name="arch" type="xml">
        	<field name="payment_id" position='after'>
        		<field name='dn_nc_id' options="{'no_open':True,'no_create_edit':True}"/>                  	                    	
        	</field>
        </field>
    </record> 
            	
	<record id="kwitansi_wizard_wtc_other_receivable_view_dn" model="ir.ui.view">
            <field name="name">wtc.kwitansi.wizard.other.receivable.dn</field>
            <field name="model">wtc.dn.nc</field>
            <field name="priority" eval="115"/>
            <field name="arch" type="xml" >
                <form string="Kwitansi">
           			 <group>
		                <field name="kwitansi_id" options="{'no_create': True,'no_open':True}" required="True"/>
		            	<field name="reason_cancel_kwitansi" attrs="{'invisible': [('cetak_ke','&lt;','1')],'required': [('cetak_ke','&gt;','0')]}"/>
		            	<field name="cetak_ke" invisible="1"/>
		             </group>
		             <group>
		                <button name="%(kwitansi_new_other_receivable)d" type="action" string="Print" class="oe_highlight"/>
		            </group>
                </form>
            </field>
        </record>
        

    </data>
</openerp>
                