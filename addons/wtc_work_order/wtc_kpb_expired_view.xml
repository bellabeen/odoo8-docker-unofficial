<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
	
	<record model="ir.ui.view" id="wtc_kpb_expired_tree_view">
		    <field name="name">wtc.kpb.expired.tree</field>
		    <field name="model">wtc.kpb.expired</field>
		    <field name="arch" type="xml">
		        <tree string="List Master KPB Expired">
		            <field name="name"/> 
		            <field name="description"/>
		            <field name="service"/>
		            <field name="hari"/>
		            <field name="km"/> 

		        </tree>
		   	</field>
		</record>
		
		
		
	<record model="ir.ui.view" id="sesi_form_view">
     <field name="name">wtc.kpb.expired.form</field>
     <field name="model">wtc.kpb.expired</field>
     <field name="arch" type="xml">
         <form string="Form KPB Expired" version="7.0">
             <group col="4">
                	<field name="name"/> 
		            <field name="description"/>
		            <field name="service"/>
		            <field name="hari"/>
		            <field name="km"/> 
             </group>
             <notebook>
				<page string="Audit Trail">
					<group>
						<group>
							<field name="create_uid" readonly="1"></field>
							<field name="create_date" readonly="1"></field>
						</group>
						<group>							
							<field name="write_uid" readonly="1"></field>
							<field name="write_date" readonly="1"></field>
						</group>
					</group>
				</page> 
             </notebook>
         </form>
     </field>
</record>

		 
		<record model="ir.actions.act_window" id="wtc_kpb_expired_action">
		    <field name="name">KPB Expired</field>
		    <field name="res_model">wtc.kpb.expired</field>
		    <field name="view_type">form</field>
		    <field name="view_mode">tree,form</field>
		</record>
		
		
 <!-- <menuitem name="KPB Expired" parent="mrp.menu_mrp_manufacturing" id="mrp_kpb_expired" action="wtc_kpb_expired_action"/>
    --> 
    
    <menuitem id="wtc_kpb_expired_form" name="KPB Expired" parent="wtc_dealer_menu.workshop_configuration_menu" sequence="1" action="wtc_kpb_expired_action"/> 
	
	</data>
</openerp>