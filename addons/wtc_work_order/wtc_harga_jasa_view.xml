<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		
		
		<record model="ir.ui.view" id="wtc_harga_jasa_version_tree_view">
		    <field name="name">wtc.harga.jasa.version.tree</field>
		    <field name="model">wtc.harga.jasa.version</field>
		    <field name="arch" type="xml">
		        <tree string="Version Harga Jasa">
		           	<field name="name"/>
		            <field name="active"/>
		            <field name="date_start"/>
			        <field name="date_end"/>
		        </tree>
		     </field>
		</record>
		
		
		<record model="ir.ui.view" id="wtc_harga_jasa_version_form_view">
		    <field name="name">wtc.harga.jasa.version.form</field>
		    <field name="model">wtc.harga.jasa.version</field>
		    <field name="arch" type="xml">
		        <form string="Form Version Harga Jasa" version="7.0">
					<label for="name" class="oe_edit_only"/>
                    <h1>
                        <field name="name"/>
                    </h1>
                    <group col="2" colspan="2">
	                    <group>
	                        <field name="date_start"/>
	                       	<field name="active"/>
	                    </group>
	                    <group>
	                       	 <field name="date_end"/>
	                    </group>
                    </group>
                    
                    <notebook>
							<page string="Audit Trail">
								<group>
									<group>
										<field name="create_uid" readonly="1"></field>
										<field name="create_date" readonly="1"></field>
									</group>
									<group>							
										<field name="write_uid" readonly="1"></field>
										<field name="write_date" readonly="1"></field>
									</group>
								</group>
							</page>	 		            	
		            </notebook>
		            
		        </form>
		    </field>
		</record>
		
		

		
		<record model="ir.ui.view" id="wtc_harga_jasa_tree_view">
		    <field name="name">wtc.harga.jasa.tree</field>
		    <field name="model">wtc.harga.jasa</field>
		    <field eval="1" name="priority"/>
		    <field name="arch" type="xml">
		        <tree string="List Harga Jasa" create="true" editable="top">
		        	<field name="harga_jasa_version_id" options="{'no_create': True,'no_open': True}"/>
		           	<field name="workshop_category" options="{'no_create': True,'no_open': True}"/>
		            <field name="product_id_jasa" options="{'no_create': True,'no_open': True}"/>
		            <field name="category_product_id" options="{'no_create': True,'no_open': True}"/>
			        <field name="price"/>
		        </tree>
		     </field>
		</record>
 
		<record model="ir.ui.view" id="wtc_harga_jasa_form_view">
		    <field name="name">wtc.harga.jasa.form</field>
		    <field name="model">wtc.harga.jasa</field>
		    <field name="arch" type="xml">
		        <form string="Form Harga Jasa" version="7.0">
		            <group>
		            	<field name="harga_jasa_version_id" options="{'no_create': True,'no_open': True}"/>
		                <field name="product_id_jasa" options="{'no_create': True,'no_open': True}"/>
		            	<field name="category_product_id" options="{'no_create': True,'no_open': True}"/>
			            <field name="price"/>
		            </group>
		            <notebook>
							<page string="Audit Trail">
								<group>
									<group>
										<field name="create_uid" readonly="1"></field>
										<field name="create_date" readonly="1"></field>
									</group>
									<group>							
										<field name="write_uid" readonly="1"></field>
										<field name="write_date" readonly="1"></field>
									</group>
								</group>
							</page>	 		            	
		            </notebook>
		        </form>
		    </field>
		</record>

		<record id="view_wtc_harga_jasa_filter" model="ir.ui.view">
            <field name="name">wtc.harga.jasa.filter</field>
            <field name="model">wtc.harga.jasa</field>
            <field name="arch" type="xml">
                <search string="Search">
                    <field name="harga_jasa_version_id"/>
                    <field name="product_id_jasa"/>
                    <field name="category_product_id"/>
                </search>
            </field>
        </record>

		<record model="ir.actions.act_window" id="action_wtc_harga_jasa_form">
		    <field name="name">Harga Jasa</field>
		    <field name="res_model">wtc.harga.jasa</field>
		    <field name="view_type">form</field>
		    <field name="view_mode">tree,form</field>
		</record>
		
		
		<record model="ir.actions.act_window" id="action_wtc_harga_jasa_version">
		    <field name="name">Harga Jasa Version</field>
		    <field name="res_model">wtc.harga.jasa.version</field>
		    <field name="view_type">form</field>
		    <field name="view_mode">tree,form</field>
		</record>

		<menuitem name="Harga Jasa" parent="wtc_dealer_menu.workshop_configuration_menu" sequence="3" id="wtc_harga_jasa_menuitem" action="action_wtc_harga_jasa_form"/>
		<menuitem name="Harga Jasa Version" parent="wtc_dealer_menu.workshop_configuration_menu" sequence="2" id="wtc_harga_jasa_version_menuitem" action="action_wtc_harga_jasa_version"/>		
	
	</data>
</openerp>