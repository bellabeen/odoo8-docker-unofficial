<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    
    	<record model="ir.ui.view" id="wtc_customer_form_wizard_view">
		    <field name="name">wtc.customer.wizard.form</field>
		    <field name="model">res.partner</field>
		    <field name="arch" type="xml">
		    <form string="Form Add Customer">
		            <group>
                        <field name="create_date" invisible="1"/>
                        <field name="name" on_change="name_wo_onchange(name)" attrs="{'readonly':[('create_date','!=',False)]}"/>
		                <field name="default_code" invisible="True"/>
		                <field name="mobile" on_change="mobile_wo_onchange(mobile)"/>
                        <field name="customer" invisible="1"/>
		            </group>
		            <group>
						<label for="address" string="Address"/>
    					<div>
                         <field name="street" required="1" placeholder="Street..." on_change="onchange_address(street,street2,rt,rw,state_id,city_id,kecamatan_id,kecamatan,zip_id,kelurahan)"/>
                            <div>
                            	<field name="street2" required="1" placeholder="Street" style="width: 50%%" on_change="onchange_address(street,street2,rt,rw,state_id,city_id,kecamatan_id,kecamatan,zip_id,kelurahan)"/>
                            	<field name="rt" required="1" placeholder="RT" style="width: 25%%" on_change="onchange_address(street,street2,rt,rw,state_id,city_id,kecamatan_id,kecamatan,zip_id,kelurahan)"/>
								<field name="rw" required="1" placeholder="RW" style="width: 25%%" on_change="onchange_address(street,street2,rt,rw,state_id,city_id,kecamatan_id,kecamatan,zip_id,kelurahan)"/>
								<field name="state_id" required="1" on_change="onchange_address(street,street2,rt,rw,state_id,city_id,kecamatan_id,kecamatan,zip_id,kelurahan)" class="oe_no_button" placeholder="Province" style="width: 50%%" options="{'no_create':True,'no_open':True}"/>
								<field name="city_id" required="1" options="{'no_create': True,'no_open': True}" on_change="onchange_address(street,street2,rt,rw,state_id,city_id,kecamatan_id,kecamatan,zip_id,kelurahan)" placeholder="City" style="width: 50%%"/>
								<field name="kecamatan_id" required="1" options="{'no_create': True,'no_open': True}" on_change="onchange_address(street,street2,rt,rw,state_id,city_id,kecamatan_id,kecamatan,zip_id,kelurahan)" placeholder="Kecamatan" style="width: 50%%"/>
								<field name="kecamatan"  on_change="onchange_address(street,street2,rt,rw,state_id,city_id,kecamatan_id,kecamatan,zip_id,kelurahan)" placeholder="Kecamatan" style="width: 50%%" />						
								<field name="zip_id" required="1" on_change="onchange_address(street,street2,rt,rw,state_id,city_id,kecamatan_id,kecamatan,zip_id,kelurahan)" placeholder="ZIP" style="width: 50%%" options='{"no_open": True}'/>                               	
                                <field name="kelurahan" on_change="onchange_address(street,street2,rt,rw,state_id,city_id,kecamatan_id,kecamatan,zip_id,kelurahan)" class="oe_no_button" placeholder="Kelurahan" style="width: 50%%"/>
                            </div>                              
                          </div>	
                        </group>
                   <group>
                   		<field name="pkp" on_change="onchange_pkp(pkp)"/>
		                <field name="npwp" attrs="{'invisible': [('pkp','!=',True)],'required': [('pkp','=',True)]}"/> 
		                <field name="alamat_pkp" attrs="{'invisible': [('pkp','!=',True)],'required': [('pkp','=',True)]}"/>
                   </group>
		        </form>
		    </field>
		</record>


        <record model="ir.ui.view" id="wtc_driver_form_wizard_view">
            <field name="name">wtc.driver.wizard.form</field>
            <field name="model">res.partner</field>
            <field name="arch" type="xml">
            <form string="Form Add Customer">
                    <group>
                        <field name="name" on_change="name_wo_onchange(name)"/>
                        <field name="default_code" invisible="True"/>
                        <field name="mobile" on_change="mobile_wo_onchange(mobile)"/>
                    </group>
                    <group>
                        <label for="address" string="Address"/>
                        <div>
                         <field name="street" required="1" placeholder="Street..." on_change="onchange_address(street,street2,rt,rw,state_id,city_id,kecamatan_id,kecamatan,zip_id,kelurahan)"/>
                            <div>
                                <field name="street2" required="1" placeholder="Street" style="width: 50%%" on_change="onchange_address(street,street2,rt,rw,state_id,city_id,kecamatan_id,kecamatan,zip_id,kelurahan)"/>
                                <field name="rt" required="1" placeholder="RT" style="width: 25%%" on_change="onchange_address(street,street2,rt,rw,state_id,city_id,kecamatan_id,kecamatan,zip_id,kelurahan)"/>
                                <field name="rw" required="1" placeholder="RW" style="width: 25%%" on_change="onchange_address(street,street2,rt,rw,state_id,city_id,kecamatan_id,kecamatan,zip_id,kelurahan)"/>
                                <field name="state_id" required="1" on_change="onchange_address(street,street2,rt,rw,state_id,city_id,kecamatan_id,kecamatan,zip_id,kelurahan)" class="oe_no_button" placeholder="Province" style="width: 50%%" options="{'no_create':True,'no_open':True}"/>
                                <field name="city_id" required="1" options="{'no_create': True,'no_open': True}" on_change="onchange_address(street,street2,rt,rw,state_id,city_id,kecamatan_id,kecamatan,zip_id,kelurahan)" placeholder="City" style="width: 50%%"/>
                                <field name="kecamatan_id" required="1" options="{'no_create': True,'no_open': True}" on_change="onchange_address(street,street2,rt,rw,state_id,city_id,kecamatan_id,kecamatan,zip_id,kelurahan)" placeholder="Kecamatan" style="width: 50%%"/>
                                <field name="kecamatan"  on_change="onchange_address(street,street2,rt,rw,state_id,city_id,kecamatan_id,kecamatan,zip_id,kelurahan)" placeholder="Kecamatan" style="width: 50%%" />                     
                                <field name="zip_id" required="1" on_change="onchange_address(street,street2,rt,rw,state_id,city_id,kecamatan_id,kecamatan,zip_id,kelurahan)" placeholder="ZIP" style="width: 50%%" options="{'no_create':True,'no_open':True}"/>                                   
                                <field name="kelurahan" on_change="onchange_address(street,street2,rt,rw,state_id,city_id,kecamatan_id,kecamatan,zip_id,kelurahan)" class="oe_no_button" placeholder="Kelurahan" style="width: 50%%"/>
                            </div>                              
                          </div>    
                        </group>
                   <group>
                        <field name="pkp" on_change="onchange_pkp(pkp)"/>
                        <field name="npwp" attrs="{'invisible': [('pkp','!=',True)],'required': [('pkp','=',True)]}"/> 
                        <field name="alamat_pkp" attrs="{'invisible': [('pkp','!=',True)],'required': [('pkp','=',True)]}"/>
                   </group>
                </form>
            </field>
        </record>






 		 <record id="view_wtc_work_order_search" model="ir.ui.view">
            <field name="name">wtc.work.order.search</field>
            <field name="model">wtc.work.order</field>
            <field name="arch" type="xml">
                <search string="Search WO">
                	<field name='name'/>
					<field name='branch_id'/>
					<field name='division'/>               	
        			<field name='type'/>
        			<field name='customer_id'/>
                    <field name='driver_id'/>
        			<field name='lot_id'/>
        			<field name="mobile" string="No HP"/> 
					<filter string="Draft" name="state_draft" domain="[('state','=','draft')]" />
					<filter string="Waiting for Approval" name="state_wfa" domain="[('state','=','waiting_for_approval')]" />
					<filter string="Confirmed" name="state_confirmed" domain="[('state','=','confirmed')]" />							
					<filter string="Approved" name="state_approved" domain="[('state','=','approved')]" />
					<filter string="Finished" name="state_finished" domain="[('state','=','finished')]" />
					<filter string="Open" name="state_open" domain="[('state','=','open')]" />        												
					<filter string="Done" name="state_done" domain="[('state','=','done')]" />        			
                    <group expand="0" string="Group By">
                        <filter string="Branch" icon="terp-go-month" domain="[]" context="{'group_by':'branch_id'}"/>
               
                    </group>
                </search>
            </field>
        </record>  		
		<record model="ir.ui.view" id="wtc_serial_number_form_wizard_view">
		    <field name="name">wtc.serial.number.wizard.form</field>
		    <field name="model">stock.production.lot</field>
		    <field name="arch" type="xml">
		    <form string="Form Add WO">
		            <group col="4">
		                <field name="name" string="Engine No" on_change="name_onchange(product_id,name)"/>
		                <field name="chassis_no" on_change="chassis_onchange(chassis_no)"/>
		                <field name="no_polisi" on_change="no_pol_onchange(no_polisi)"/>
		                <field name="nama_buku" on_change="nama_buku_onchange(nama_buku)"/>
		            </group>
		            <group col="4">
		            	<field name="product_id" />
		                <field name="customer_id" required="True" context="{'form_view_ref':'wtc_work_order.wtc_customer_form_wizard_view'}"/>
		                <field name="kode_buku" on_change="kode_buku_onchange(kode_buku)"/>
		                <field name="state" invisible="True"/>
		            </group>
		        </form>
		    </field>
		</record>
		
		
		


        <record id="view_wtc_work_order_tree" model="ir.ui.view">
            <field name="name">work.order.tree</field>
            <field name="model">wtc.work.order</field>
            <field name="arch" type="xml">
                <tree string="Work Order">
	                <field name="name"/>
	                <field name="date"/>
	                <field name="no_pol"/>
	                <field name='lot_id'/>
	                <field name='customer_id'/>
                    <field name='driver_id'/>
	                <field name='mobile'/>
	                <field name="state"/>
	                <field name="state_wo"/>
                </tree>
            </field>
        </record>	
		
		
		<record id="wizard_unused_view" model="ir.ui.view">
            <field name="name">wtc.work.order.unused</field>
            <field name="model">wtc.work.order</field>
            <field name="arch" type="xml">
                <form string="Reason">
                    <group>
                    <field name="reason_unused"/>
                    </group>
             
                    <footer>
		               <button name="unused_request" string="Confirm" type="workflow" class="oe_highlight"/>
		                or
		                <button string="Discard" class="oe_link" special="cancel" />
		            </footer>
		            	
                </form>
            </field>
        </record>
       		
		
        <record id="view_wtc_work_order_form" model="ir.ui.view">
            <field name="name">work.order.form</field>
            <field name="model">wtc.work.order</field>
            <field name="arch" type="xml">
                <form string="Work Order">
                
                <header>
                	
                	 <button name="action_tidak_digunakan" states="draft" string="Unused" type="object" class="oe_highlight"/>
                	 <button name="view_picking" string="Supply Products" type="object" attrs="{'invisible': ['|', ('shipped','=',True), ('state','!=', 'approved')]}" class="oe_highlight"/>
                     <button name="view_picking_slip" type="object" attrs="{'invisible': [('type_wo','!=',True)]}"  string="View Picking" class="oe_highlight" />
                     <button name="invoice_create_wo"  type="object"  states="finished" string="Create invoice" class="oe_highlight" />
                     <button name="view_invoice"  type="object" states="open,done" string="View Invoice" class="oe_highlight" />
                     <button name="start_stop_wo"  type="object" string="Start Stop WO" attrs="{'invisible': ['|', ('state_wo','=','finish'), ('state','!=', 'approved')]}" class="oe_highlight"/>
                     <button name="picking_ok" states="except_picking" string="Manually Corrected"/>
                     <button name="invoice_ok" states="except_invoice" string="Manually Corrected"/>
                     <button name="%(report_work_order)d" states="open,done" type="action" string="Print WO" class="oe_highlight"  icon="gtk-print"/>
                     
                     <button name="%(report_work_order_picking)d" type="action" string="Print Picking" attrs="{'invisible': [('state','not in', ('approved','finished','open','done'))]}" icon="gtk-print" class="oe_highlight"/>

                     <button name="%(report_work_order_invoice)d" states="open,done" type="action" string="Print Invoice" class="oe_highlight"  icon="gtk-print"/>
                    
                    <button name="action_clocking_reset" attrs="{'invisible':['|',('state','!=','approved'),('state_wo','!=','finish')]}" string="Reset Clocking" type="object" groups="wtc_work_order.group_button_reset_clocking"/>
                    
                    <field name="state" widget="statusbar" statusbar_visible="draft,waiting_for_approval,approved,finished,open,done"/>	
                
                </header>
               
				<sheet>
                    <div class="oe_title">                      
	       				<h1>
				            <label string="Work Order"/>
				 			<field name="name" class="oe_inline" readonly="1"/>	
    			        </h1>
                    </div>
                    <group col="2" colspan="2">
                    	<group>
                    		<field name="branch_id" options="{'no_show':True,'no_create':True,'no_open':True}" on_change="onchange_branch_id(branch_id,product_id)" attrs="{'readonly': [('state','!=','draft')]}"/>
                    	</group>
                    	<group>
                    		<field name="division" attrs="{'readonly': [('state','!=','draft')]}"/>
                    	</group>
                    	<group>
                            <label for="type"/>
                            <div>
                                <field name="type" class="oe_inline" attrs="{'readonly': [('state','!=','draft')]}"/>
                                <field name="kpb_ke" attrs="{'invisible': [('type','!=','KPB')], 'required': [('type','=','KPB')], 'readonly': [('state','!=','draft')]}" class="oe_inline"/>
                                <field name="is_event_kpb" invisible='1' class="oe_inline"/>
                                <field name="prev_work_order_id" options="{'no_show':True,'no_create':True,'no_open':True}"  attrs="{'invisible': [('type','!=','WAR')],'required': [('type','=','WAR')]}" class="oe_inline" on_change="onchange_prev_work_order_id(prev_work_order_id)" domain="[('id','=',0)]"/>
                            </div>
                            <field name="lot_id" domain="[('state','in',['stock','sold', 'paid','workshop','sold_offtr','paid_offtr','False'])]" context="{'form_view_ref':'wtc_work_order.wtc_serial_number_form_wizard_view'}" on_change="onchange_lot_id(lot_id)" attrs="{'required': [('type','!=','SLS')],'readonly': [('state','!=','draft')]}" />
                            <!--<field name="lot_id"  domain="[('state','in',['paid','workshop','sold_offtr'])]" context="{'form_view_ref':'wtc_work_order.wtc_serial_number_form_wizard_view'}" on_change="onchange_lot_id(lot_id)" attrs="{'required': [('type','!=','SLS')],'readonly': [('state','!=','draft')]}" />-->
                            <field name="chassis_no" on_change="chassis_onchange(chassis_no)" attrs="{'readonly': [('state','!=','draft')]}"/>
                            <field name="no_pol" on_change="no_pol_onchange(no_pol)" attrs="{'readonly': [('state','!=','draft')]}"/>
	                    	<!-- <field name="kode_buku" on_change="kode_buku_onchange(kode_buku)" attrs="{'readonly': [('state','!=','draft')]}"/>
	                    	<field name="nama_buku" on_change="nama_buku_onchange(nama_buku)" attrs="{'readonly': [('state','!=','draft')]}"/> -->
                            <field name="pekerjaan" attrs="{'readonly': [('state','!=','draft')]}" options="{'no_show':True,'no_create':True,'no_open':True}"/>
	                    	<field name="km" attrs="{'readonly': [('state','!=','draft')],'required': [('type','!=','SLS')]}"/>
	                    	<field name="tanggal_pembelian" attrs="{'required':[('type','!=','SLS')],'readonly':[('state','!=','draft')]}" />                         
                            <field name="warranty"/>
		                	<field name='dealer_sendiri' widget='radio' string='Dealer Sendiri' attrs="{'required': [('type','!=','SLS')],'readonly':[('state','!=','draft')]}"/>
		                	<field name='hubungan_pemilik' string='Hub. Dengan Pemilik' attrs="{'required': [('type','!=','SLS')],'readonly':[('state','!=','draft')]}"/>
		                	<field name='alasan_ke_ahass' widget='radio'  string='Alasan Ke Ahass' attrs="{'required': [('type','!=','SLS')],'readonly':[('state','!=','draft')]}"/>
						</group>
                    	<group>
                    		<field name="date" readonly='1'/>
                    		<field name="date_confirm" readonly="1"/>
                            <field name="customer_id" context="{'form_view_ref':'wtc_work_order.wtc_customer_form_wizard_view'}"  attrs="{'readonly': [('state','!=','draft')]}" required="1"/>
                            <field name='driver_id' context="{'form_view_ref':'wtc_work_order.wtc_driver_form_wizard_view'}"  attrs="{'readonly': [('state','!=','draft')],'required': [('type','!=','SLS')]}" on_change="onchange_driver_id(driver_id,type,branch_id)"/>
                            <field name="mobile" required="1" attrs="{'readonly':[('state','!=','draft')]}"/>
							<field name="product_id" options="{'no_show':True,'no_create':True,'no_open':True}"  attrs="{'required': [('type','!=','SLS')],'readonly': [('state','!=','draft')]}"/>
							<field name="member" attrs="{'readonly': [('state','!=','draft')]}"/>
                            <field name="bensin" attrs="{'required': [('type','!=','SLS')],'readonly': [('state','!=','draft')]}"/>
	                    	<field name="mekanik_id" attrs="{'readonly': [('state','!=','draft')]}" on_change="mekanik_id_change(mekanik_id)" options="{'no_open':True,'no_create':True}" domain="[('branch_ids','=',branch_id)]"/>
                            <field name='tahun_perakitan'  attrs="{'required': [('type','!=','SLS')],'readonly':[('state','!=','draft')]}" on_change="onchange_tahun_perakitan(tahun_perakitan,date)" />
		                	<field name='employee_id' invisible='1'/>
                            <field name="type_customer" attrs="{'required':[('type','=','SLS'),('date','>=','2019-12-04')],'invisible':[('type','!=','SLS')],'readonly':[('state','!=','draft')]}"/>
		                </group>
		            </group>
                    <notebook colspan="5">
                        <page string="Order lines">
                           <!--  <field colspan="4" name="work_lines" nolabel="1" widget="one2many_list" attrs="{'readonly': [('state','!=','draft')]}"> -->
                                <field colspan="4" name="work_lines" nolabel="1" widget="one2many_list" attrs="{'readonly': ['|',('state_wo','=','finish')]}" >
                                <tree string=""  editable="top" >
                                	<field name="categ_id"  string="Categ" on_change="category_change(categ_id,parent.type,parent.kpb_ke,parent.branch_id,parent.division,parent.product_id)" attrs="{'readonly': [('state','!=','draft')]}"/>
					                <field name="product_id" options="{'no_show':True,'no_create':True,'no_open':True}"  on_change="product_change(product_id,categ_id,parent.branch_id,parent.product_id,parent.kpb_ke,parent.type)" attrs="{'readonly': [('state','!=','draft')]}"/>
					                <field name="warranty" invisible='1' on_change="create_warranty(warranty)"/>
					                <field name="warranty_show" readonly='1' />
					                <field name="name" invisible='1' on_change="create_name(name)"/>
					                <field name="name_show" readonly='1' />
					                <field name="location_id" attrs="{'readonly': ['|',('categ_id','=','Service'),('state','!=','draft')],'required': [('categ_id','=','Sparepart')]}" options="{'no_show':True,'no_create':True,'no_open':True}"  on_change="location_change(location_id,product_id,categ_id,parent.branch_id)" domain="[('branch_id','=',parent.branch_id),('usage','=','internal')]"/>
					                <field name="qty_available_show" readonly='1' />
					                <field name="qty_available" invisible='1' on_change="create_qty_available(qty_available)"/>
					                <field name="product_uom" invisible='1' options="{'no_show':True,'no_create':True,'no_open':True}" />
					                <field name="product_qty" attrs="{'readonly': [('state','!=','draft')]}"/>
					                <field name="supply_qty" invisible='1' on_change="create_supply_qty(supply_qty)"/>
					                <field name="supply_qty_show" readonly='1'/>

					                <field name="discount" attrs="{'readonly': [('state','!=','draft')]}"/>
                                    <field name="state" invisible='1'/>

                                   

					                <field name="price_unit_show" readonly='1' />
					               	<field name="price_unit" invisible='1' on_change="create_price(price_unit)"/>
	                              	<field name="tax_id" invisible='1' on_change="change_tax(tax_id)" options="{'no_show':True,'no_create':True,'no_open':True}"  widget="many2many_tags" domain="[('parent_id','=',False),('type_tax_use','&lt;&gt;','sale')]"/>				               
	                              	<field name="tax_id_show" readonly="1" options="{'no_open':True,'no_create_edit':True}" widget="many2many_tags" domain="[('parent_id','=',False),('type_tax_use','&lt;&gt;','sale')]"/>				               
	                              							               
	                              	<field name="price_subtotal"/>
					               
                                </tree>
                            </field>
                            
                             <group class="oe_subtotal_footer oe_right" colspan="2" name="work_total">
                             	<field name="amount_untaxed" widget='monetary' options="{'currency_field': 'currency_id'}"/>
                             	<field name="amount_tax" widget='monetary' options="{'currency_field': 'currency_id'}"/>
                             	<div class="oe_subtotal_footer_seperator oe_inline">
                             	<label for="amount_total"/>
                             	<button name="button_dummy" string="(update)" type="object" class="oe_edit_only oe_link" attrs="{'invisible':[('amount_total','!=',0)]}"/>
                             	</div>
                             	<field name="amount_total" nolabel="1" class="oe_subtotal_footer_separator" widget='monetary' options="{'currency_field': 'currency_id'}"/>
                             </group>
                             
                             
                             <div class="oe_clear"/>
                             <field name="note" class="oe_inline" placeholder="Keluhan..."/>
                        </page>
                        <page string='History Service' >
				 			<group>
				 				<field name='work_order_history_ids'/>
				 			</group>
				 		</page> 
				 		<page string='Faktur Pajak' attrs="{'invisible': [('amount_tax','=',0)]}"  >
				 			<group>
				 				<field name='pajak_gabungan' attrs="{'readonly': [('state','!=','draft')]}"></field>
				 				<field name='faktur_pajak_id' readonly='1'></field>
				 			</group>
				 		</page>                        
                        <page string="Other Information" position="inside">
                        		<group> 
					        		<field name="payment_term" readonly="1"/>
					        		<field name="shipped" groups="base.group_no_one"/>
					        		<field invisible="True" name="type_wo" groups="base.group_no_one"/>
                        		</group>
                        		<group col="4"> 
					        		<field name="start"/>
					        		<field name="date_break"/>
					        		<field name="finish"/>
					        		<field name="end_break"/>
                        		</group>
                        </page>
                    </notebook>
                </sheet> 
                </form>
            </field>
        </record>
        
 
		
		
        
        <record id="view_account_journal_wo_reg" model="ir.ui.view">
            <field name="name">wtc.branch.config.reg</field>
            <field name="model">wtc.branch.config</field>
            <field name="inherit_id" ref="wtc_branch.wtc_branch_config_form_view"/>
            <field name="arch" type="xml">
                <field name="branch_id" position="after">
                	<group string="Work Order">
                		<field name="wo_kpb_journal_id"/>
                		<field name="wo_claim_journal_id"/>
	                    <field name="wo_reg_journal_id"/>
	                    <field name="wo_war_journal_id"/>
	                    <field name="wo_collecting_kpb_journal_id"/>
                        <field name="wo_collecting_claim_journal_id"/>
	                </group>
                </field>
            </field>		
