<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
        <record id="view_teds_department_act_budget_tree" model="ir.ui.view">
			<field name="name">teds.department.act.budget.tree</field>
			<field name="model">hr.department</field>
			<field name="inherit_id" ref="hr.view_department_tree"/>
			<!-- <field name="type">tree</field>
			<field name="priority" eval="10" /> -->
			<field name="arch" type="xml">					
				<!-- <xpath expr="//field[@name='complete_name']" position="before">
					<field name="branch_id" />
				</xpath> -->
				<xpath expr="//field[@name='company_id']" position="before">
					<field name="department_code" />
				</xpath>
			</field>
		</record>

		<record id="view_teds_department_act_budget_form" model="ir.ui.view">
			<field name="name">teds.department.act.budget.form</field>
			<field name="model">hr.department</field>
			<field name="inherit_id" ref="hr.view_department_form"/>
			<!-- <field name="type">tree</field>
			<field name="priority" eval="10" /> -->
			<field name="arch" type="xml">					
				<!-- <xpath expr="//field[@name='name']" position="before">
					<field name="branch_id" required="1" options="{'no_create': True, 'no_open': True}" />
				</xpath> -->
				<xpath expr="//field[@name='manager_id']" position="before">
					<field name="department_code" />
				</xpath>				
			</field>
		</record>
    </data>
</openerp>