<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
		<record id="view_teds_api_log_tree" model="ir.ui.view">
            <field name="name">teds.api.log.tree</field>
            <field name="model">teds.api.log</field>
            <field name="arch" type="xml">
                <tree string="Api Log" create='false' delete='false' edit='false'>
                    <field name="name"/>
					<field name="description"/>
                    <field name="module_name"/>
                    <field name="date"/>
                    <field name="model_name"/>
                    <field name="origin"/>
                </tree>
            </field>
        </record>

        <record id="view_teds_api_log_form" model="ir.ui.view">
            <field name="name">teds.api.log.form</field>
            <field name="model">teds.api.log</field>
            <field name="arch" type="xml">
                <form string="Api Log" create='false' delete='false' edit='false'>
                    <sheet>
                        <group colspan="4" col="4">
                            <field name="name" readonly="1"/>
                            <field name="date" readonly="1"/>
                            <field name="description" readonly="1"/>
                            <field name="module_name" readonly="1"/>
                            <field name="status" readonly="1"/>
                            <field name="origin" readonly="1"/>
                            <field name="model_name" readonly="1"/>
                            <field name="transaction_id" readonly="1"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="teds_api_log_action" model="ir.actions.act_window">
            <field name="name">Api Log</field>
            <field name="res_model">teds.api.log</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        
        <menuitem id='teds_api_log_menuitem' parent='wtc_dealer_menu.ma_branches_menu' action='teds_api_log_action' sequence='101'/>
    </data>
</openerp>