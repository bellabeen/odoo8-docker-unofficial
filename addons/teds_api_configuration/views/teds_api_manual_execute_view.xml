<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data> 

			<record model="ir.ui.view" id="teds_api_manual_excute_tree_view">
			    <field name="name">teds.api.manual.execute.api.tree</field>
			    <field name="model">teds.api.manual.execute.api</field>
			    <field name="arch" type="xml">
			        <tree string="API Manual Excute">
			            <field name="name"/>
			        </tree>
			     </field>
			</record>
			
			<record model="ir.ui.view" id="teds_api_manual_excute__form_view">
			    <field name="name">teds.api.manual.execute.api.form</field>
			    <field name="model">teds.api.manual.execute.api</field>
			    <field name="arch" type="xml">
			        <form col="4" string="API Manual Excute" >
                        <group>
                            <field name="jenis_api"/>
                            <field name="name"/>
                        </group>
                        <footer>
                            <button name="action_proses" string="Proses" type="object" class="oe_highlight" />
                            or 
                            <button string="Cancel" class="oe_link" special="cancel" />
                        </footer>
                </form>
			    </field>
			</record> 
			




		<record model="ir.actions.act_window" id="teds_api_manual_excute_action">
		    <field name="name">API Manual Excute</field>
		    <field name="res_model">teds.api.manual.execute.api</field>
		    <field name="view_type">form</field>
		    <field name="view_mode">form</field>
		    <field name="target">new</field>
		</record>




			<menuitem id="teds_api_manual_excute__menu" name="API Manual Excute" sequence="3" parent="wtc_dealer_menu.ma_branches_menu" action="teds_api_manual_excute_action"/> 
	</data>
</openerp> 
