<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data> 

			<record model="ir.ui.view" id="teds_api_list_partner_tree_view">
			    <field name="name">teds.api.list.partner.tree</field>
			    <field name="model">teds.api.list.partner</field>
			    <field name="arch" type="xml">
			        <tree string="API List Partner">
			            <field name="name"/>
			        </tree>
			     </field>
			</record>
			
			<record model="ir.ui.view" id="teds_api_list_partner_form_view">
			    <field name="name">teds.api.list.partner.form</field>
			    <field name="model">teds.api.list.partner</field>
			    <field name="arch" type="xml">
			        <form string="API List Partner">
			        	<sheet>
			             <group col="4" colspan="4">
				            <field name="name"/>       
						</group>
					<notebook>	
						<page string="Partner">
		                    <field name="partner_ids" nolabel="1" colspan="4">
		                        <tree string="Partner" editable="bottom">
		                        	<field name="name"/>
		                            <!-- <field name="default_code"/> -->
		                        </tree>
		                    </field>
		                  
		                </page>
						             
	                </notebook>
	                	</sheet>
			        </form>
			    </field>
			</record> 
	 		<record id="teds_api_list_partner_filter" model="ir.ui.view">
	            <field name="name">teds.api.list.partner.list.select</field>
	            <field name="model">teds.api.list.partner</field>
	            <field name="arch" type="xml">
	                <search string="Search Area">
	                    <field name="name"/>
	                </search>
	            </field>
	        </record>			
			<record model="ir.actions.act_window" id="teds_api_list_partner_action">
			    <field name="name">API Partner List</field>
			    <field name="res_model">teds.api.list.partner</field>
			    <field name="view_type">form</field>
			    <field name="view_mode">tree,form</field>
				<field name="search_view_id" ref="teds_api_list_partner_filter"/>					    
			</record>

			<menuitem id="teds_api_list_partner_menu" name="API Partner List" sequence="3" parent="wtc_dealer_menu.ma_branches_menu" action="teds_api_list_partner_action"/> 
	</data>
</openerp> 
