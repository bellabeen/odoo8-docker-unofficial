<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id='view_teds_api_button_tree' model='ir.ui.view'>
            <field name='name'>teds.api.button.tree</field>
            <field name='model'>teds.api.button</field>
            <field name="arch" type="xml">
                <tree string="Api Button" create='false' edit='false' delete='false'>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id='view_teds_api_button_form' model='ir.ui.view'>
            <field name='name'>teds.api.button.form</field>
            <field name='model'>teds.api.button</field>
            <field name="arch" type="xml">
            	<form string="Api Button" create='false' edit='false' delete='false'>
                    <header name="header_button">
                    </header>
                    <sheet>
                        <group>
        					<field name="name" readonly="1"/>
                        </group>
                    </sheet>
            	</form>
            </field>
        </record>

        <record id="teds_api_button_action" model="ir.actions.act_window">
            <field name="name">Api Button Auto</field>
            <field name="res_model">teds.api.button</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        
        <menuitem id='teds_api_all_button_menuitem' parent='wtc_dealer_menu.ma_branches_menu' action='teds_api_button_action'/>
    </data>
</openerp>