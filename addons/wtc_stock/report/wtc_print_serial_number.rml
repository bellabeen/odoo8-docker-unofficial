    <paraStyle name="terp_isi_table_line_right" fontName="Helvetica" fontSize="8.0" leading="0.5cm" alignment="RIGHT" spaceBefore="0.0" spaceAfter="0.0"/>
    <paraStyle name="terp_parag" fontName="Helvetica" fontSize="9.0" leading="0.5cm" alignment="LEFT" spaceBefore="0.0" spaceAfter="0.0"/>
    <paraStyle name="terp_isi_table_kecil" fontName="Helvetica" fontSize="7.0" leading="0.3cm" alignment="LEFT" spaceBefore="0.0" spaceAfter="0.0"/>

</stylesheet>

<story>
<para style="header">[[repeatIn(objects, 'p')]]<pageCount/></para>
	<blockTable colWidths="11cm,8.5cm" style="Table_No_Line">
	[[ p.rel_division in ['Sparepart','Umum'] and removeParentNode('story') ]]
   		<tr>
   			<td>
   				<para style="terp_header_besar">[[ p.rel_branch_id.company_id.name]] [[ p.rel_branch_id.name]] </para>
   				<blockTable colWidths="13cm" style="Table_No_Line">
	      	   		<tr>
	      	   			<td><para style="terp_header_kecil">[[ p.rel_branch_id.street ]]</para></td>
	      	   		</tr>
	      	   		<tr>
	      	   			<td><para style="terp_header_kecil">[[ p.rel_branch_id.state_id.name ]]</para></td>
	      	   		</tr>
				</blockTable>
   			</td>
   			
   			<td>
   			<para style="terp_header_besar">Serial Number Available</para>
   			<blockTable colWidths="2cm,0.5cm,7cm" style="Table_No_Line">
	      	   	<tr>
	      	   		<td><para style="terp_header_kecil">Tanggal</para></td>
	      	   		<td><para style="terp_header_kecil">:</para></td>
	      	   		<td><para style="terp_header_kecil">[[waktu_local()]]</para></td>
	      	   	</tr>
			</blockTable>
   			</td>
   		</tr>
	</blockTable>
	
<nextFrame name="frame_content"/>
	<blockTable colWidths="0.8cm,1.5cm,2.8cm,2.5cm,3.3cm,3.3cm,3.5cm,2cm" style="Table_Line">
   		<tr>
   			<td><para style="terp_isi_table_line">No </para></td>
   			<td><para style="terp_isi_table_line">Kode</para></td>
   			<td><para style="terp_isi_table_line">Keterangan</para></td>
			<td><para style="terp_isi_table_line">Warna</para></td>
			<td><para style="terp_isi_table_line">No Mesin</para></td>
			<td><para style="terp_isi_table_line">No Rangka</para></td>
			<td><para style="terp_isi_table_line">Lokasi</para></td>
			<td><para style="terp_isi_table_line">Tanggal Masuk</para></td>
   		</tr>
	</blockTable>
	<section>
	
    <para style="terp_default">[[ repeatIn(serial_number_ids(), 'x') ]]</para>
	<blockTable colWidths="0.8cm,1.5cm,2.8cm,2.5cm,3.3cm,3.3cm,3.5cm,2cm" style="Table_Line">
   		<tr>
   			<td><para style="terp_isi_table_line_right"><seq id="number"/></para></td>
   			<td><para style="terp_isi_table_line">[[x.product_id.name]]</para></td>
   			<td><para style="terp_isi_table_line">[[x.product_id.description]]</para></td>
   			<td><para style="terp_isi_table_line">[[x.product_id.attribute_value_ids.name]]</para></td>
			<td><para style="terp_isi_table_line">[[x.name]]</para></td>
			<td><para style="terp_isi_table_line">[[x.chassis_no]]</para></td>
			<td><para style="terp_isi_table_line">[[loc_name(x.location_id)]]</para></td>
			<td><para style="terp_isi_table_line">[[in_date(x.id)]]</para></td>
   		</tr>
	</blockTable>
	</section>
	<para><seqReset id="number"/></para>
	
</story>
</document>
