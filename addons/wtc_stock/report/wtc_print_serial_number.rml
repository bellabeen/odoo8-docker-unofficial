<!DOCTYPE document SYSTEM "rml.dtd">
<document filename="print_serial_number.pdf">

<template pageSize="(21cm, 29.7cm)" showBoundary="0" title="Payments Request"  allowSplitting="20">
<pageTemplate id="Layout1">
	
	<!-- three frames are defined here: -->
	<frame id="frame_header" x1="00.0" y1="27.4cm" width="21cm" height="2.1cm"/>
	<frame id="frame_content" x1="00.0" y1="7.4cm" width="21cm" height="20cm"/>
	<frame id="frame_footer" x1="00.0" y1="2.8cm" width="21cm" height="4.5cm"/>

</pageTemplate>
</template>
