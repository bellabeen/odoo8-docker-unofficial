<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		
		<record model="ir.ui.view" id="wtc_stock_move_form_view">
			<field name="name">wtc.stock.move.form</field>
		    <field name="model">stock.move</field>
		    <field name="inherit_id" ref="stock.view_move_form"/>
		    <field name="arch" type="xml">
		    	<button name="action_cancel" position="attributes">
		    		<attribute name="invisible">1</attribute>
		    	</button>
		    	<button name="action_done" position="attributes">
		    		<attribute name="invisible">1</attribute>
		    	</button>
		    	<field name="product_id" position="before">
		    		<field name="branch_id" options="{'no_open':True,'no_create':True}" required="True" attrs="{'readonly':[('state','!=','draft')]}"/>
		    		<field name="categ_id" on_change="categ_id_change(branch_id,func_division,categ_id,location_id)" options="{'no_open':True,'no_create':True}" required="True" attrs="{'readonly':[('state','!=','draft')]}"/>
		    		<field name="func_division" invisible="1"/>
		    	</field>
		    	<field name="name" position="after">
		    		<field name="restrict_lot_id" domain="[('product_id','=',product_id),('location_id','=',location_id)]" options="{'no_open':True,'no_create':True}" on_change="lot_qty_change(product_uom_qty,product_uom,restrict_lot_id)" attrs="{'required': [('func_division','=','Unit')], 'invisible': [('func_division','!=','Unit')]}"/>
		    	</field>
		    	<field name="product_uom_qty" position="attributes">
		    		<attribute name="on_change">lot_qty_change(product_uom_qty,product_uom,restrict_lot_id)</attribute>
		    	</field>
		    	<field name="product_uom" position="attributes">
		    		<attribute name="on_change">lot_qty_change(product_uom_qty,product_uom,restrict_lot_id)</attribute>
		    	</field>
		    	<field name="location_id" position="attributes">
		    		<attribute name="domain">[('usage','&lt;&gt;','view'),('maximum_qty', '!=', 0),('branch_id','=',branch_id)]</attribute>
		    		<attribute name="on_change">categ_id_change(branch_id,func_division,categ_id,location_id</attribute>
		    	</field>
		    	<field name="location_dest_id" position="attributes">
		    		<attribute name="domain">[('usage','=','internal'),('maximum_qty', '!=', 0),('branch_id','=',branch_id),('end_date','&gt;=', context_today().strftime('%%Y-%%m-%%d')),('start_date','&lt;=',context_today().strftime('%%Y-%%m-%%d'))]</attribute>
		    	</field>
				<xpath expr="//sheet" position="inside">
					<notebook>
						<page string="Audit Trail">
							<group>
								<group>
									<field name="create_uid" readonly="1"></field>
									<field name="create_date" readonly="1"></field>
									<field name="confirm_uid" readonly="1"></field>
									<field name="confirm_date" readonly="1"></field>
								</group>
								<group>							
									<field name="write_uid" readonly="1"></field>
									<field name="write_date" readonly="1"></field>
									<field name="cancel_uid" readonly="1"></field>
									<field name="cancel_date" readonly="1"></field>
								</group>
							</group>
						</page>
					</notebook>
				</xpath>		    	
		    </field>
		</record>
		
		<record model="ir.ui.view" id="wtc_stock_move_picking_form_view">
			<field name="name">wtc.stock.move.picking.form</field>
		    <field name="model">stock.move</field>
		    <field name="inherit_id" ref="stock.view_move_picking_form"/>
		    <field name="arch" type="xml">
		    	
		    	<field name="product_id" position="before">
		    		<field name="branch_id" options="{'no_open':True,'no_create':True}" required="True" domain="[('id','=',parent.branch_id)]" on_change="picking_branch_id_change(categ_id,parent.branch_id,parent.division,parent.picking_type_id,location_id,location_dest_id)"/>
		    		<field name="func_division" invisible="1"/>
		    	</field>
		    	<field name="product_id" position="after">
		    		<field name="categ_id" options="{'no_open':True,'no_create':True}" required="True" on_change="onchange_product_id(product_id,location_id,location_dest_id,parent.partner_id)"/>
		    	</field>
		    	<field name="name" position="after">
		    		<field name="restrict_lot_id" domain="[('product_id','=',product_id),('location_id','=',location_id)]" options="{'no_open':True,'no_create':True}" on_change="lot_qty_change(product_uom_qty,product_uom,restrict_lot_id)" attrs="{'required': [('func_division','=','Unit')], 'invisible': [('func_division','!=','Unit')]}"/>
		    	</field>
		    	<field name="product_uom_qty" position="attributes">
		    		<attribute name="on_change">lot_qty_change(product_uom_qty,product_uom,restrict_lot_id)</attribute>
		    	</field>
		    	<field name="product_uom" position="attributes">
		    		<attribute name="on_change">lot_qty_change(product_uom_qty,product_uom,restrict_lot_id)</attribute>
		    	</field>
		    	<field name="location_id" position="attributes">
		    		<attribute name="domain">[('usage','&lt;&gt;','view'),('maximum_qty', '!=', 0),('branch_id','=',branch_id)]</attribute>
		    		<attribute name="on_change">picking_branch_id_change(categ_id,parent.branch_id,parent.division,parent.picking_type_id,location_id,location_dest_id)</attribute>
		    	</field>
		    	<field name="location_dest_id" position="attributes">
		    		<attribute name="domain">[('usage','=','internal'),('maximum_qty', '!=', 0),('branch_id','=',branch_id),('end_date','&gt;=', context_today().strftime('%%Y-%%m-%%d')),('start_date','&lt;=',context_today().strftime('%%Y-%%m-%%d'))]</attribute>
		    	</field>
		    	<button name="do_unreserve" position="attributes">
		    		<attribute name="invisible">1</attribute>
		    	</button>
		    	
		    </field>
		</record>
		
		<record model="ir.ui.view" id="wtc_stock_move_picking_tree_view">
			<field name="name">wtc.stock.move.picking.tree</field>
		    <field name="model">stock.move</field>
		    <field name="inherit_id" ref="stock.view_move_picking_tree"/>
		    <field name="arch" type="xml">
		    	
		    	<field name="availability" position="replace">
		    		
		    	</field>
		    	<field name="reserved_availability" position="replace">
		    		
		    	</field>
		    	
		    	<field name="remaining_qty" position="replace">
		    		
		    	</field>
		    	
		    	<field name="string_availability_info" position="replace">
		    		
		    	</field>
		    	
		    </field>
		</record>
		
		<record model="ir.ui.view" id="wtc_stock_move_tree_view">
			<field name="name">wtc.stock.move.tree</field>
		    <field name="model">stock.move</field>
		    <field name="inherit_id" ref="stock.view_move_tree"/>
		    <field name="arch" type="xml">
		    	
		    	<button name="action_done" position="attributes">
		    		<attribute name="invisible">1</attribute>
		    	</button>
		    	
		    </field>
		</record>
    
         <menuitem action="stock.action_warehouse_form" id="menu_warehouse_master"  parent="wtc_dealer_menu.ma_warehouse_menu" sequence="60"/> 
      <menuitem action="stock.action_location_form" id="menu_location_master"  parent="wtc_dealer_menu.ma_warehouse_menu" sequence="70"/> 
      <menuitem action="stock.action_production_lot_form" id="menu_serial_number_master"  parent="wtc_dealer_menu.ma_traceability_menu" sequence="70"/> 
     <menuitem action="stock.action_move_form2" id="menu_stock_move_master"  parent="wtc_dealer_menu.ma_traceability_menu" sequence="70"/>
     <menuitem action="stock.quantsact" id="menu_stock_quant_master"  parent="wtc_dealer_menu.ma_traceability_menu" sequence="70"/>
     
     
      
		
	</data>
</openerp>