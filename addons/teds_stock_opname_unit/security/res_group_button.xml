            <field name="implied_ids" eval="[(4, ref('group_teds_stock_opname_unit_read'))]"/>
        </record>

        <record id="wtc_branch.group_sys_admin" model="res.groups">
			<field name="implied_ids" eval="[
				(4,ref('group_teds_so_unit_posted_button')),
				(4,ref('group_teds_so_unit_print_validasi_button')),
				(4,ref('group_teds_so_unit_print_bakso_button')),
