            <field name="implied_ids" eval="[(4, ref('group_teds_stock_opname_unit_read'))]"/>
        </record>     
        <record id="group_teds_so_unit_print_validasi_button" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Teds Stock Opname Unit Print Validasi</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_stock_opname_unit_read'))]"/>
        </record>     
        <record id="group_teds_so_unit_print_bakso_button" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
