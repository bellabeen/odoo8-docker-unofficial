<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">    
        <record id="group_teds_so_unit_generate_stock_button" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Teds Stock Opname Unit Generate Stock</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_stock_opname_unit_read'))]"/>
        </record>     
        <record id="group_teds_so_unit_posted_button" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
