<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">  	
    	
<!-- Report Mutasi -->	
	
        <record id="group_wtc_report_mutasi_read" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Report Mutasi Read</field> 
            <field name="menu_access" eval="[(4,ref('mutasi_part_menu')),
											(4,ref('mutasi_unit_menu'))
											]"/>  
        </record> 
                      
        <record id="group_wtc_report_mutasi_update" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Report Mutasi Update</field> 
            <field name="menu_access" eval="[(4,ref('mutasi_part_menu')),
											(4,ref('mutasi_unit_menu'))
											]"/>     
			<field name="implied_ids" eval="[(4, ref('group_wtc_report_mutasi_read'))]"/>            	            	
        </record>
        
        <record id="wtc_branch.group_sys_admin" model="res.groups">
			<field name="implied_ids" eval="[(4,ref('group_wtc_report_mutasi_read')),
											(4,ref('group_wtc_report_mutasi_update'))
											]"/> 
		</record>                                                                                                    
    </data>
</openerp>