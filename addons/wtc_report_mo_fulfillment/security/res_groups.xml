        <record id="wtc_branch.group_sys_admin" model="res.groups">
			<field name="implied_ids" eval="[(4,ref('group_laporan_mo_read')),
											(4,ref('group_laporan_mo_update')),

											]"/> 
		</record>                                                                                                  
    </data>
