        <record id="group_laporan_mo_read" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Laporan MO Fulfillment Read</field> 
            <field name="menu_access" eval="[(4, ref('menu_mo_fulfillment_report_unit')),
            								(4, ref('menu_mo_fulfillment_report_part'))
            								]"/>  
        </record> 
                
        <record id="group_laporan_mo_update" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Laporan MO Fulfillment Update</field> 
            <field name="menu_access" eval="[(4, ref('menu_mo_fulfillment_report_part')),
            								(4, ref('menu_mo_fulfillment_report_unit'))
            								]"/>   
			<field name="implied_ids" eval="[(4, ref('group_laporan_mo_read'))]"/>            	            	
