<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
	
		<record model="ir.ui.view" id="wtc_warehouse_form_view">
		    <field name="name">wtc.warehouse.form</field>
		    <field name="model">stock.warehouse</field>
		    <field name="inherit_id" ref="stock.view_warehouse"/>
		    <field name="arch" type="xml">
		    
		    	<field name="code" position="after">
		    		<field name="branch_id" required="True"/>
		    	</field>
		    	
		    	<field name="out_type_id" position="after">
		    		<field name="interbranch_in_type_id" domain="[('code','=','internal')]" readonly="True"/>
		    		<field name="interbranch_out_type_id" domain="[('code','=','internal')]" readonly="True"/>
		    	</field>
		    	
		    	<field name="name" position="attributes">
		    		<attribute name="on_change">upper_change(name,code)</attribute>
		    	</field>
		    	
		    	<field name="code" position="attributes">
		    		<attribute name="on_change">upper_change(name,code)</attribute>
		    		<attribute name="attrs">{'invisible':[('rel_code','!=',False)]}</attribute>
		    	</field>
		    	
		    	<field name="code" position="after">
		    		<field name="rel_code" attrs="{'invisible':[('rel_code','=',False)]}"/>
		    	</field>
            	<xpath expr="//sheet" position="inside">
            		<notebook>
						<page string="Audit Trail">
							<group>
								<group>
									<field name="create_uid" readonly="1"></field>
									<field name="create_date" readonly="1"></field>
								</group>
								<group>							
									<field name="write_uid" readonly="1"></field>
									<field name="write_date" readonly="1"></field>
								</group>
							</group>
						</page>
					</notebook>
				</xpath>		    	
		     </field>
		</record>
		
		<record model="ir.ui.view" id="wtc_warehouse_location_form_view">
		    <field name="name">wtc.warehouse.location.form</field>
		    <field name="model">stock.location</field>
		    <field name="inherit_id" ref="stock.view_location_form"/>
		    <field name="arch" type="xml">
		    
		    	<field name="active" position="after">
		    		<field name="warehouse_id" invisible="1"/>
		    	</field>
		    	
		     </field>
		</record>
		
		<record model="ir.ui.view" id="wtc_warehouse_ir_sequence_form_view">
		    <field name="name">wtc.warehouse.ir.sequence.form</field>
		    <field name="model">ir.sequence</field>
		    <field name="inherit_id" ref="base.sequence_view"/>
		    <field name="arch" type="xml">
		    	
		    	<field name="active" position="after">
		    		<field name="picking_type_id" invisible="1"/>
		    	</field>
		    	
		     </field>
		</record>
		
    	 <record id="wtc_all_operation_tree_view" model="ir.ui.view">
            <field name="name">stock.picking.type.custom.tree.view</field>
            <field name="model">stock.picking.type</field>
            <field name="inherit_id" ref="stock.view_picking_type_tree"/>
            <field name="arch" type="xml">
				<field name="name" position="after">
					<field name="branch_id"/>
					<field name="code"/>
					<field name="return_picking_type_id"/>
					<field name="default_location_src_id"/>
					<field name="default_location_dest_id"/>
				</field>
            </field>
        </record>
            
    	 <record id="wtc_all_operation_search_view" model="ir.ui.view">
            <field name="name">stock.picking.type.custom.search.view</field>
            <field name="model">stock.picking.type</field>
            <field name="inherit_id" ref="stock.view_pickingtype_filter"/>
            <field name="arch" type="xml">
				<field name="name" position="after">
					<field name="branch_id"/>
					<field name="code"/>
					<field name="return_picking_type_id"/>
					<field name="default_location_src_id"/>
					<field name="default_location_dest_id"/>
				</field>
            </field>
        </record>
                    
        <record model="ir.actions.act_window" id="stock.action_picking_type_form">
            <field name="view_mode">tree,form</field>
        </record>
        		
	</data>
</openerp>