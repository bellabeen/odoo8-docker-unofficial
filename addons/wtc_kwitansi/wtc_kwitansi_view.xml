<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    	
		<record model="ir.ui.view" id="wtc_kwitansi_form_view">
			<field name="name">wtc.kwitansi.form</field>
		    <field name="model">account.voucher</field>
		    <field name="type">form</field>
		    <field name="inherit_id" ref="account_voucher.view_vendor_receipt_form"/>
		    <field name="arch" type="xml">
		    	<data>
		    		<xpath expr="//button[@name='cancel_voucher']" position="after">
	        			<button name="%(print_kwitansi)d" type="action" class="oe_highlight" string="Print" icon="gtk-print" attrs="{'invisible': [('state','!=','posted')]}"/>
	        		</xpath>
		    	</data>
		    </field>
		</record>
		
    </data>
</openerp>