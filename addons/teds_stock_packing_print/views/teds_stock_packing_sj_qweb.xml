<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>
<template id="teds_stock_packing_sj_qweb">
    <t t-call="report.html_container">
        <t t-foreach="docs" t-as="o">
            <div class="page">
                <div class="row">
                    <table width="100%" style="font-size: 11px;">
                        <tr>
                            <th width="60%">
                                <span t-esc="o.rel_branch_id.company_id.name"/> <span t-esc="o.rel_branch_id.name"/>
                            </th>
                            <th>
                                <t t-if="o.rel_division == 'Sparepart'">
                                    BUKTI PENERIMAAN BARANG
                                </t>
                                <t t-if="o.rel_division != 'Sparepart'">
                                    Surat Jalan
                                </t>

                            </th>
                        </tr>
                        <tr>
                            <td>
                                <span t-esc="o.rel_branch_id.street"/>
                                <br/>
                                <span t-esc="o.rel_branch_id.state_id.name"/>
                            </td>
                            <td>
                                <table width="100%">
                                    <tr>
                                        <td width="20%">No </td>
                                        <td>: <span t-esc="o.name"/></td>
                                    </tr>
                                    <tr>
                                        <t t-if="o.rel_division != 'Sparepart'">
                                            <td width="20%">Tanggal </td>
                                            <td>: <span t-field="o.date" t-options='{"format": "dd-mm-yyyy"}'/></td>
                                        </t>
                                    </tr>
                                </table>
                            </td>
                        </tr>
                    </table>
                    <table width="100%" style="border: 1px solid; border-collapse: collapse; font-size: 11px;">
                        <t t-if="o.rel_division != 'Sparepart'">
                            <tr>
                                <td width="50%">
                                    <table width="100%">
                                        <tr>
                                            <td width="30%" style="padding: 2px">Kepada Yth</td>
                                            <td>: <span t-esc="mutation_order()[0]"/></td>
                                        </tr>
                                        <tr>
                                            <td style="padding: 2px">Alamat</td>
                                            <td>: <span t-esc="mutation_order()[1]"/></td>
                                        </tr>
                                        <tr>
                                            <td style="padding: 2px">No Invoice</td>
                                            <td>: <span t-esc="invoice()"/></td>
                                        </tr>
                                    </table>
                                </td>
                            
                                <td width="50%" style="padding: 5px;">
                                    <table width="100%">
                                        <tr>
                                            <td width="30%" style="text-align:right; padding: 2px">Diambil</td>
                                        </tr>
                                        <tr>
                                            <td style="padding: 2px">No Pol</td>
                                            <td>: <span t-esc="o.plat_number_id.plat_number"/></td>
                                        </tr>
                                        <tr>
                                            <td style="padding: 2px">Sopir</td>
                                            <td>: <span t-esc="o.driver_id.driver"/></td>
                                        </tr>
                                    </table>
                                </td>
                            </tr>
                        </t>
                        <t t-if="o.rel_division == 'Sparepart'">
                            <tr>
                                <td style="padding: 5px;" colspan="2">
                                    <table width="100%">
                                        <tr>
                                            <td width="20%" style="padding: 2px">Tanggal</td>
                                            <td>: <span t-field="o.date" t-options='{"format": "dd-mm-yyyy"}'/></td>
                                            <t t-if="order_po() != False">
                                                <!-- No PO DMS Mutasi -->
                                                <td width="10%" style="padding: 2px">Origin PO</td>
                                                <td text-align="right">: <span t-esc="order_po()"/></td>
                                            </t>
                                        </tr>
                                        <tr>
                                            <td style="padding: 2px">Supplier</td>
                                            <td>: <span t-esc="mutation_order()[0]"/></td>
                                        </tr>
                                        <tr>
                                            <td style="padding: 2px">Lokasi Gedung</td>
                                            <td>: <span t-esc="o.rel_picking_type_id.warehouse_id.name"/>-<span t-esc="o.rel_picking_type_id.name"/></td>
                                        </tr>
                                        <tr>
                                            <td style="padding: 2px">No Invoice</td>
                                            <td>: <span t-esc="invoice()"/>-<span t-esc="o.rel_picking_type_id.name"/></td>
                                        </tr>
                                    </table>
                                </td>
                            </tr>
                        </t>
                        <tr>
                            <td colspan="2">
                                <t t-if="o.rel_division != 'Sparepart'">
                                    <table width="100%" style="border: 1px solid; border-collapse: collapse; font-size:11px">
                                        <tr>
                                            <td style="padding: 3px; border: 1px solid; text-align:center;">No</td>
                                            <td style="padding: 3px; border: 1px solid; text-align:center;">No PO</td>
                                            <td style="padding: 3px; border: 1px solid; text-align:center;">Tgl Inv</td>
                                            <td style="padding: 3px; border: 1px solid; text-align:center;">Kode</td>
                                            <td style="padding: 3px; border: 1px solid; text-align:center;">Keterangan</td>
                                            <t t-if="o.rel_division == 'Unit'">
                                                <td style="padding: 3px; border: 1px solid; text-align:center;">Warna</td>
                                                <td style="padding: 3px; border: 1px solid; text-align:center;">No Mesin</td>
                                                <td style="padding: 3px; border: 1px solid; text-align:center;">No Rangka</td>
                                            </t>
                                        </tr>
                                        <tr t-foreach="o.packing_line" t-as="x">
                                            <td style="padding: 3px; border: 1px solid;"><span t-esc="no_urut()"/></td>
                                            <td style="padding: 3px; border: 1px solid;"><span t-esc="o.picking_id.origin"/></td>
                                            <td style="padding: 3px; border: 1px solid;"><span t-field="o.date" t-options='{"format": "dd-mm-yyyy"}'/></td>
                                            <td style="padding: 3px; border: 1px solid;"><span t-esc="x.product_id.name"/></td>
                                            <td style="padding: 3px; border: 1px solid;"><span t-esc="x.product_id.description"/></td>
                                            <t t-if="o.rel_division == 'Unit'">
                                                <td style="padding: 3px; border: 1px solid;"><span t-esc="x.product_id.attribute_value_ids.name"/></td>
                                                <td style="padding: 3px; border: 1px solid;"><span t-esc="x.serial_number_id.name"/></td>
                                                <td style="padding: 3px; border: 1px solid;"><span t-esc="x.serial_number_id.chassis_no"/></td>
                                            </t>
                                        </tr>
                                    </table>
                                </t>
                                <t t-if="o.rel_division == 'Sparepart'">
                                    <table width="100%" style="border: 1px solid; border-collapse: collapse; font-size:11px">
                                        <tr>
                                            <td style="padding: 3px; border: 1px solid; text-align:center;">No</td>
                                            <td style="padding: 3px; border: 1px solid; text-align:center;">Kode Barang</td>
                                            <td style="padding: 3px; border: 1px solid; text-align:center;">Nama Barang</td>
                                            <td style="padding: 3px; border: 1px solid; text-align:center;">Kondisi</td>
                                            <td style="padding: 3px; border: 1px solid; text-align:center;">Jml</td>
                                            <td style="padding: 3px; border: 1px solid; text-align:center;">Jml Pack</td>
                                            <td style="padding: 3px; border: 1px solid; text-align:center;">No Picking</td>
                                            <td style="padding: 3px; border: 1px solid; text-align:center;">No PO</td>
                                            <td style="padding: 3px; border: 1px solid; text-align:center;">Lokasi</td>
                                        </tr>
                                        <tr t-foreach="o.packing_line" t-as="x">
                                            <td style="padding: 3px; border: 1px solid;"><span t-esc="no_urut()"/></td>
                                            <td style="padding: 3px; border: 1px solid;"><span t-esc="x.product_id.name"/></td>
                                            <td style="padding: 3px; border: 1px solid;"><span t-esc="x.product_id.default_code"/></td>
                                            <td style="padding: 3px; border: 1px solid;">OK</td>
                                            <td style="padding: 3px; border: 1px solid;"><span t-esc="x.quantity"/></td>
                                            <td style="padding: 3px; border: 1px solid;"><span t-esc="x.quantity"/></td>
                                            <td style="padding: 3px; border: 1px solid;"><span t-esc="o.picking_id.name"/></td>
                                            <td style="padding: 3px; border: 1px solid;"><span t-esc="o.picking_id.origin"/></td>
                                            <td style="padding: 3px; border: 1px solid;"><span t-esc="x.destination_location_id.name"/></td>
                                        </tr>
                                    </table>
                                </t>
                            </td>
                        </tr>
                    </table>
                    <t t-if="o.rel_division != 'Sparepart'"><span style="font-size: 11px;">Ket: Dibawa Oleh Bp</span></t>
                    <table width="100%" style="font-size: 11px;">
                        <tr height="45px">
                            <td width="20%">
                                <t t-if="o.rel_division != 'Sparepart'">Dicetak Oleh</t>
                                <t t-if="o.rel_division == 'Sparepart'"><br/>Pengirim</t>

                            </td>
                            <td width="20%">
                                <t t-if="o.rel_division != 'Sparepart'">Pengirim</t>
                                <t t-if="o.rel_division == 'Sparepart'"><br/>Kepala Gudang</t>
                            </td>
                        </tr>
                        <tr height="11px">
                            <td>_____________________</td>
                            <td>_____________________</td>
                            <t t-if="o.rel_division == 'Sparepart'">
                                <td style="font-size:11px" colspan="2" width="60%">
                                    <span t-esc="print_user()"/> <span t-esc="waktu_local()"/><br/>
                                    Cetakan Ke: <span t-esc="jumlah_cetakan()"/>
                                </td>
                            </t>
                        </tr>
                        <t t-if="o.rel_division != 'Sparepart'">
                            <tr height="15px">
                                <td><span t-esc="print_user()"/> <span t-esc="waktu_local()"/></td>
                            </tr>
                        </t>
                        <tr>
                            <td colspan="3">
                                <t t-if="o.rel_division != 'Sparepart'">Lembar ke-1 : Pengirim / Lembar ke-2 : Penerima</t>
                                <t t-if="o.rel_division == 'Sparepart'">Lembar ke-1 Pengirim / Lembar ke-2 Gudang / Lembar ke-3 Finance</t>
                            </td>
                        </tr>
                    </table>
                </div>                  
            </div>
        </t>
    </t>
</template>
</data>
</openerp>
