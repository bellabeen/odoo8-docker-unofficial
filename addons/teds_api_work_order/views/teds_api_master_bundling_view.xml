<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
        <record model="ir.ui.view" id="teds_api_master_partner_bundling_tree_view">
            <field name="name">teds.api.master.partner.bundling.tree</field>
            <field name="model">teds.api.master.partner.bundling</field>
            <field name="arch" type="xml">
                <tree string="Partner Bundling">
                    <field name="branch_id" />
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="teds_api_master_partner_bundling_form_view">
            <field name="name">teds.api.master.partner.bundling.form</field>
            <field name="model">teds.api.master.partner.bundling</field>
            <field name="arch" type="xml">
                <form string="Partner Bundling">
                    <div class="oe_title">
                        <label for="branch_id" class="oe_edit_only" />                      
                        <h1>
                            <field name="branch_id" required="1" style="max-width: none;" options="{'no_create':True, 'no_open':True}" /> 
                        </h1>
                    </div>
                    <notebook>
                        <page string="Partner Bundling">
                            <field name="partner_bundling_ids" nolabel="1" colspan="4">
		                    	<tree string="Partner Bundling" editable="top">
		                    		<field name="partner_id" />
		                    	</tree>
		                    </field>
                        </page>
                    </notebook>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="teds_api_master_partner_bundling_action">
            <field name="name">API Partner Bundling</field>
            <field name="res_model">teds.api.master.partner.bundling</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem id="teds_api_master_partner_bundling_menuitem" name="API Partner Bundling" sequence="102" parent="wtc_dealer_menu.ma_branches_menu" action="teds_api_master_partner_bundling_action" />
    </data>
</openerp>