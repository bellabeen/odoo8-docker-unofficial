<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
	
	
	
	<record model="ir.ui.view" id="view_wizard_eksport_ahm_rec">
            <field name="name">view.wizard.eksport.ahm.rec</field>
            <field name="model">eksport.ahm.rec</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form col="4" string="Eksport AHM REC" version="7.0">
                    <group>
                        
                        <field name="name" invisible="True"/>
                        <group col="2" colspan="2">
	                    <group>
	                        <field name="date_start" required="1"/>
	                    </group>
	                	<group>
	                        <field name="date_end" required="1"/>
	                    </group>
                    </group>
                        <separator colspan="4"/>
                        <field name="data_file" nolabel="1" colspan="4" filename="name"/>
                    </group>
                    <footer>
                        <button name="export_file" string="Export" type="object" class="oe_highlight"/>
                        or 
                        <button string="Cancel" class="oe_link" special="cancel" />
                        <button name="eksport_auto_distribution" string="Auto REC" type="object" class="oe_highlight"/>
                    </footer>
                </form>
            </field>
        </record>
        
        <record model="ir.actions.act_window" id="action_export_ahm_rec">
            <field name="name">Generate File AHM REC</field>
            <field name="res_model">eksport.ahm.rec</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

	<menuitem id="generate_ahm_rec_submenu"   parent="wtc_dealer_menu.workshop_report_menu"  sequence="105"  action="action_export_ahm_rec"/>
	        
	</data>
</openerp>