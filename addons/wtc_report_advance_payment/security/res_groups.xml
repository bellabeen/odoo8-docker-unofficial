        </record>

        <record id="wtc_branch.group_sys_admin" model="res.groups">
			<field name="implied_ids" eval="[(4,ref('group_report_adp_read')),
												(4,ref('group_report_adp_update')),

											]"/> 
		</record>                                                                
    </data>
