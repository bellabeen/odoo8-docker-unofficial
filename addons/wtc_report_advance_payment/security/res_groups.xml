        <record id="group_report_adp_read" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Report Advance Payment Read</field> 
            <field name="menu_access" eval="[(4, ref('menu_report_advance_payment'))]"/>  
        </record> 
        <record id="access_group_report_adp_read_journal" model="ir.model.access">
            <field name="model_id" ref="account.model_account_journal"/>
            <field name="name">report_adp_journal read</field> 
            <field name="group_id" ref="group_report_adp_read"/>  
            <field name="perm_read">1</field> 
        </record>           
        <record id="group_report_adp_update" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Report Advance Payment Update</field> 
            <field name="menu_access" eval="[(4, ref('menu_report_advance_payment'))]"/>  
