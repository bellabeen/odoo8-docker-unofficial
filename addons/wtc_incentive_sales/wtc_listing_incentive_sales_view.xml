<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record model="ir.ui.view" id="wtc_listing_incentive_sales_tree_view">
			<field name="name">wtc.listing.incentive.sales.tree.view</field>
			<field name="model">wtc.listing.incentive.sales</field>
			<field name="arch" type="xml">
				<tree string="Listing Incentive Sales">
					<field name="sales_force"/>	
					<field name="cluster"/>
				</tree>
			</field>
		</record>
		
		<record model="ir.ui.view" id="wtc_listing_incentive_sales_form_view">
			<field name="name">wtc_listing_incentive_sales_form_view</field>
			<field name="model">wtc.listing.incentive.sales</field>
			<field name="arch" type="xml">
				<form string="Form Listing Incentive Sales" version="7.0">
				<group>
					<group>
						<field name="sales_force"/>
						<field name="cluster"/>
					</group>
				</group>
				 <notebook >
	                <page string="Incentive Line" >
	                	<field name="listing_incentive_sales_lines">                    			
                    		<tree editable="top">
                    			<field name="qty" />
								<field name="cash" />
                    			<field name="credit" />			
                    			<field name="reward" />
                    		</tree>
	                    </field>
                            <div class="oe_clear"/>
	                 </page>
					<page string="Audit Trail">
						<group>
							<group>
								<field name="create_uid" readonly="1"></field>
								<field name="create_date" readonly="1"></field>
							</group>
							<group>							
								<field name="write_uid" readonly="1"></field>
								<field name="write_date" readonly="1"></field>
							</group>
						</group>
					</page>		
						                 
		            </notebook>	
		            

				</form>
			</field>
		</record>
		
		
		 <record id="wtc_branch_cluster_form_view" model="ir.ui.view">
            <field name="name">wtc.branch.cluster.incentive.sales</field>
            <field name="model">wtc.branch</field>
            <field name="inherit_id" ref="wtc_branch.wtc_branch_category_form_view" />
		    <field name="arch" type="xml">
		        <field name="workshop_category" position="before">
		            <field name="cluster"/>
		        </field>
		    </field>
        </record>
        
        
		
		<record model="ir.actions.act_window" id="wtc_listing_incentive_sale_action_view">
			<field name="name">Listing Incentive Sales</field>
			<field name="res_model">wtc.listing.incentive.sales</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
		</record>
		
		
		
		<record id="view_import_absensi_wizard" model="ir.ui.view">
		<field name="name">Import Absensi</field>
		<field name="model">wtc.import.absensi</field>
		<field name="type">form</field>
		<field name="arch" type="xml">
	        <form string="Import Absensi">
	        	<group>
                   <field name="data_file" nolabel="1" colspan="4"/>
            	</group>
	        	
                  <footer>
                        <button name="import_excel" string="Import" type="object" class="oe_highlight" />
                        or 
                        <button string="Cancel" class="oe_link" special="cancel" />
                 </footer>
	          	
	        </form>
    	</field>
    </record>

    <record id="action_import_absensi" model="ir.actions.act_window">
		<field name="name">Import Absensi</field>
		<field name="type">ir.actions.act_window</field>
		<field name="res_model">wtc.import.absensi</field>
		<field name="view_type">form</field>
		<field name="view_mode">form</field>
		<field name="target">new</field>
    </record>
    
    
		
		<menuitem action="wtc_listing_incentive_sale_action_view" id="wtc_listing_incentive_sales_menu" parent="wtc_dealer_menu.showroom_configuration_menu" />
       	<menuitem id="wtc_import_absensi_submenu"  parent="hr.menu_hr_main"  action="action_import_absensi" sequence="11"/> 		
		
		
	</data>
</openerp>