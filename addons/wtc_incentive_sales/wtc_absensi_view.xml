<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    
        <record id="wtc_absensi_tree_view" model="ir.ui.view">
            <field name="name">wtc.absensit.tree.view</field>
            <field name="model">wtc.absensi</field>
            <field name="arch" type="xml">
                <tree string="Absensi" create="false">
	                <field name="nip"/>
	                <field name="bulan"/>
	                <field name="total_absensi"/>
	                <field name="jumlah_hari_kerja"/>	
	               <field name="sp"/>
                </tree>
            </field>
        </record>	
        
        <record id="action_wtc_absensi" model="ir.actions.act_window">
             <field name="res_model">wtc.absensi</field>
             <field name="type">ir.actions.act_window</field>
             <field name="view_type">form</field>
             <field name="view_mode">tree,form</field>
             <field name="name">Absensi</field>
       </record>
        

   
	    <menuitem id="wtc_absensi_menu"  parent="hr.menu_hr_main" sequence="100"  action="action_wtc_absensi"/>
        
	</data>
</openerp>
