<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
                
<!-- Penyerahan STNK Post -->
		
        <record id="group_wtc_penyerahan_sntk_post" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Penyerahan STNK Post</field>
            <field name="implied_ids" eval="[(4, ref('group_penyerahan_stnk_read'))]"/>
        </record>
                   
        <record id="access_wtc_penyerahan_sntk_post_write" model="ir.model.access">
            <field name="model_id" ref="model_wtc_penyerahan_stnk"/>
            <field name="name">penyerahan_stnk_post write</field> 
            <field name="group_id" ref="group_wtc_penyerahan_sntk_post"/>  
            <field name="perm_write">1</field> 
        </record>
           
        <record id="access_wtc_penyerahan_sntk_line_lot_write" model="ir.model.access">
            <field name="model_id" ref="stock.model_stock_production_lot"/>
            <field name="name">penyerahan_sntk_stock_lot write</field> 
            <field name="group_id" ref="group_wtc_penyerahan_sntk_post"/>  
            <field name="perm_write">1</field> 
            <field name="perm_read">1</field> 
        </record> 

<!-- Penyerahan BPKB Post -->
		
        <record id="group_wtc_penyerahan_bpkb_post" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Penyerahan BPKB Post</field>
            <field name="implied_ids" eval="[(4, ref('group_penyerahan_bpkb_read'))]"/>
        </record>
                   
        <record id="access_wtc_penyerahan_bpkb_post_write" model="ir.model.access">
            <field name="model_id" ref="model_wtc_penyerahan_bpkb"/>
            <field name="name">penyerahan_bpkb_post write</field> 
            <field name="group_id" ref="group_wtc_penyerahan_bpkb_post"/>  
            <field name="perm_write">1</field> 
        </record>
           
        <record id="access_wtc_penyerahan_bpkb_line_lot_write" model="ir.model.access">
            <field name="model_id" ref="stock.model_stock_production_lot"/>
            <field name="name">penyerahan_bpkb_stock_lot write</field> 
            <field name="group_id" ref="group_wtc_penyerahan_bpkb_post"/>  
            <field name="perm_write">1</field> 
            <field name="perm_read">1</field> 
        </record>
                
        <record id="wtc_branch.group_sys_admin" model="res.groups">
			<field name="implied_ids" eval="[(4,ref('group_wtc_penyerahan_sntk_post')),
											(4,ref('group_wtc_penyerahan_bpkb_post')),
											]"/> 
		</record>                         
    </data>
</openerp>
