<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data noupdate="1">
		<!-- STOCK IDEAL -->	
		<record id="group_stock_ideal_read" model="res.groups">
    	<field name="category_id" ref="wtc_branch.tdm_role"/>
      <field name="name">Stock Ideal Read</field> 
      <field name="menu_access" eval="[(4, ref('stock_ideal_menu'))]"/>  
		</record>
		<record id="access_stock_ideal_read" model="ir.model.access">
    	<field name="model_id" ref="model_stock_ideal"/>
      <field name="name">stock_ideal read</field> 
      <field name="group_id" ref="group_stock_ideal_read"/>  
      <field name="perm_read">1</field> 
		</record>
		<record id="access_stock_ideal_line_read" model="ir.model.access">
      <field name="model_id" ref="model_stock_ideal_line"/>
      <field name="name">stock_ideal_line read</field> 
      <field name="group_id" ref="group_stock_ideal_read"/>  
      <field name="perm_read">1</field> 
    </record> 


		<record id="group_stock_ideal_update" model="res.groups">
    	<field name="category_id" ref="wtc_branch.tdm_role"/>
      <field name="name">Stock Ideal Update</field> 
      <field name="menu_access" eval="[(4, ref('stock_ideal_menu'))]"/>
      <field name="implied_ids" eval="[(4, ref('group_stock_ideal_read'))]"/>  
		</record>
		<record id="access_stock_ideal_update" model="ir.model.access">
    	<field name="model_id" ref="model_stock_ideal"/>
      <field name="name">stock_ideal update</field> 
      <field name="group_id" ref="group_stock_ideal_update"/>  
			<field name="perm_create">1</field> 
      <field name="perm_write">1</field> 
		</record>
    <record id="access_stock_ideal_line_update" model="ir.model.access">
      <field name="model_id" ref="model_stock_ideal_line"/>
      <field name="name">stock_ideal_line update</field> 
      <field name="group_id" ref="group_stock_ideal_update"/>  
      <field name="perm_read">1</field> 
    </record> 

		
		<record id="group_stock_ideal_delete" model="res.groups">
    	<field name="category_id" ref="wtc_branch.tdm_role"/>
      <field name="name">Stock Ideal Delete</field> 
      <field name="menu_access" eval="[(4, ref('stock_ideal_menu'))]"/>
      <field name="implied_ids" eval="[(4, ref('group_stock_ideal_read'))]"/>  
		</record>
		<record id="access_stock_ideal_delete" model="ir.model.access">
    	<field name="model_id" ref="model_stock_ideal"/>
      <field name="name">stock_ideal delete</field> 
      <field name="group_id" ref="group_stock_ideal_delete"/>  
			<field name="perm_unlink">1</field>
		</record>
     <record id="access_stock_ideal_line_delete" model="ir.model.access">
      <field name="model_id" ref="model_stock_ideal_line"/>
      <field name="name">stock_ideal_line delete</field> 
      <field name="group_id" ref="group_stock_ideal_delete"/>  
      <field name="perm_read">1</field> 
    </record> 

    <record id="wtc_branch.group_sys_admin" model="res.groups">
       <field name="implied_ids" eval="[(4,ref('group_stock_ideal_read')),
                                             (4,ref('group_stock_ideal_update')),
                                             (4,ref('group_stock_ideal_delete'))
                      ]"/> 
    </record>   

	</data>
</openerp>