<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
    
<!-- Unit Bundlind -->
		
        <record id="group_wtc_unit_bundling_read" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Unit Bundling Read</field>
            <field name="menu_access" eval="[
            								(4, ref('menu_unit_bundling'))
            								]"/>
        </record>
        
        <record id="access_wtc_unit_bundling_read" model="ir.model.access">
            <field name="model_id" ref="model_wtc_unit_bundling"/>
            <field name="name">wtc_unit_bundling read</field>
            <field name="group_id" ref="group_wtc_unit_bundling_read"/>
            <field name="perm_read">1</field>
        </record>
		
        <record id="access_wtc_unit_bundling_line_read" model="ir.model.access">
            <field name="model_id" ref="model_wtc_unit_bundling_line"/>
            <field name="name">wtc_unit_bundling_line read</field>
            <field name="group_id" ref="group_wtc_unit_bundling_read"/>  
            <field name="perm_read">1</field>
        </record>
        
        <record id="access_wtc_unit_bundling_approval_line_read" model="ir.model.access">
            <field name="model_id" ref="wtc_approval.model_wtc_approval_line"/>
            <field name="name">wtc_unit_bundling_approval_line read</field>
            <field name="group_id" ref="group_wtc_unit_bundling_read"/>  
            <field name="perm_read">1</field>
        </record>
        
        <record id="group_wtc_unit_bundling_update" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Unit Bundling Update</field>
			<field name="implied_ids" eval="[(4, ref('group_wtc_unit_bundling_read'))]"/>
        </record>
        
        <record id="access_wtc_unit_bundling_update" model="ir.model.access">
            <field name="model_id" ref="model_wtc_unit_bundling"/>
            <field name="name">wtc_unit_bundling update</field> 
            <field name="group_id" ref="group_wtc_unit_bundling_update"/>  
            <field name="perm_create">1</field> 
            <field name="perm_write">1</field> 
        </record>      
        
        <record id="access_wtc_unit_bundling_line_update" model="ir.model.access">
            <field name="model_id" ref="model_wtc_unit_bundling_line"/>
            <field name="name">wtc_unit_bundling_line update</field> 
            <field name="group_id" ref="group_wtc_unit_bundling_update"/>  
            <field name="perm_create">1</field>
            <field name="perm_write">1</field>
            <field name="perm_delete">1</field>
        </record>
        
        <record id="access_wtc_unit_bundling_approval_line_update" model="ir.model.access">
            <field name="model_id" ref="wtc_approval.model_wtc_approval_line"/>
            <field name="name">wtc_unit_bundling_approval_line update</field>
            <field name="group_id" ref="group_wtc_unit_bundling_update"/>
            <field name="perm_create">1</field>
            <field name="perm_write">1</field>
        </record>
		
        <record id="group_wtc_unit_bundling_delete" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Unit Bundling Delete</field> 
			<field name="implied_ids" eval="[(4, ref('group_wtc_unit_bundling_read'))]"/>            	            	
        </record>
        
		<record id="access_wtc_unit_bundling_delete" model="ir.model.access">
            <field name="model_id" ref="model_wtc_unit_bundling"/>
            <field name="name">wtc_unit_bundling delete</field>
            <field name="group_id" ref="group_wtc_unit_bundling_delete"/>
            <field name="perm_unlink">1</field>
        </record>
        
        <record id="access_wtc_unit_bundling_line_delete" model="ir.model.access">
            <field name="model_id" ref="model_wtc_unit_bundling_line"/>
            <field name="name">wtc_mutation_request_line delete</field>
            <field name="group_id" ref="group_wtc_unit_bundling_delete"/>
            <field name="perm_unlink">1</field>
        </record>
        
        
        

                
        <record id="wtc_branch.group_sys_admin" model="res.groups">
			<field name="implied_ids" eval="[(4,ref('group_wtc_unit_bundling_read')),
											(4,ref('group_wtc_unit_bundling_update')),
											(4,ref('group_wtc_unit_bundling_delete')),
											]"/> 
		</record>         
    </data>
</openerp>
