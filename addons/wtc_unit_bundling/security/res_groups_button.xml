<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data noupdate="1">
	
	

		<record id="group_wtc_unit_bundling_rfa" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Unit Bundling RFA</field>
            <field name="implied_ids" eval="[(4, ref('group_wtc_unit_bundling_read'))]"/>
        </record>
        
        <record id="access_wtc_unit_bundling_rfa" model="ir.model.access">
            <field name="model_id" ref="model_wtc_unit_bundling"/>
            <field name="name">wtc_unit_bundling_request_rfa update</field>
            <field name="group_id" ref="group_wtc_unit_bundling_rfa"/>
            <field name="perm_write">1</field>
        </record>
        
        <record id="access_wtc_unit_bundling_approval_line_rfa" model="ir.model.access">
            <field name="model_id" ref="wtc_approval.model_wtc_approval_line"/>
            <field name="name">wtc_unit_bundling_approval_line_rfa write</field>
            <field name="group_id" ref="group_wtc_unit_bundling_rfa"/>
            <field name="perm_create">1</field>
        </record>
        
        <!-- Unit Bundlind Cancel Approval -->

		<record id="group_wtc_unit_bundling_cancel_approval" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Unit Bundling Cancel Approval</field>
            <field name="implied_ids" eval="[(4, ref('group_wtc_unit_bundling_read'))]"/>
        </record>
        
        <record id="access_wtc_unit_bundling_cancel_approval" model="ir.model.access">
            <field name="model_id" ref="model_wtc_unit_bundling"/>
            <field name="name">wtc_unit_bundling_cancel_approval update</field>
            <field name="group_id" ref="group_wtc_unit_bundling_cancel_approval"/>
            <field name="perm_write">1</field>
        </record>
        
        <record id="access_wtc_unit_bundling_approval_line_cancel" model="ir.model.access">
            <field name="model_id" ref="wtc_approval.model_wtc_approval_line"/>
            <field name="name">wtc_unit_bundling_approval_line_cancel write</field>
            <field name="group_id" ref="group_wtc_unit_bundling_cancel_approval"/>
            <field name="perm_write">1</field>
        </record>
        
        <!-- Unit Bundlind Cancel -->

		<record id="group_wtc_unit_bundling_cancel" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Unit Bundling Cancel</field>
            <field name="implied_ids" eval="[(4, ref('group_wtc_unit_bundling_read'))]"/>
        </record>
        
        <record id="access_wtc_unit_bundling_cancel" model="ir.model.access">
            <field name="model_id" ref="model_wtc_unit_bundling"/>
            <field name="name">wtc_unit_bundling_cancel update</field>
            <field name="group_id" ref="group_wtc_unit_bundling_cancel"/>
            <field name="perm_write">1</field>
        </record>
        
        
        <record id="group_wtc_unit_bundling_confirm" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Unit Bundling Confirm</field>
            <field name="implied_ids" eval="[(4, ref('group_wtc_unit_bundling_read'))]"/>
        </record>
             
        <record id="access_wtc_unit_bundling_confirm_write" model="ir.model.access">
            <field name="model_id" ref="model_wtc_unit_bundling"/>
            <field name="name">wtc_unit_bundling_confirm write</field> 
            <field name="group_id" ref="group_wtc_unit_bundling_confirm"/>  
            <field name="perm_write">1</field> 
        </record>   
        

        
      
        
     
        
     
        
        
        <record id="wtc_branch.group_sys_admin" model="res.groups">
			<field name="implied_ids" eval="[(4,ref('group_wtc_unit_bundling_rfa')),
				(4,ref('group_wtc_unit_bundling_cancel_approval')),
				(4,ref('group_wtc_unit_bundling_approve')),
				(4,ref('group_wtc_unit_bundling_cancel')),
				]">
			</field>
		</record>
	
	</data>
</openerp>