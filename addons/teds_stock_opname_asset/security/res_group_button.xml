            <field name="implied_ids" eval="[(4, ref('group_teds_stock_opname_asset_read'))]"/>
        </record>

        <record id="wtc_branch.group_sys_admin" model="res.groups">
			<field name="implied_ids" eval="[
                (4,ref('group_teds_so_asset_generate_stock_button')),
                (4,ref('group_teds_so_asset_posted_button')),
                (4,ref('group_teds_so_asset_print_validasi_button')),
                (4,ref('group_teds_so_asset_print_bakso_button')),
