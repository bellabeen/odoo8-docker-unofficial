<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">    
        <record id="group_teds_so_asset_generate_stock_button" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Teds Stock Opname Asset Generate Stock</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_stock_opname_asset_read'))]"/>
        </record>     
        <record id="group_teds_so_asset_posted_button" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Teds Stock Opname Asset Post</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_stock_opname_asset_read'))]"/>
        </record>     
        <record id="group_teds_so_asset_print_validasi_button" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
