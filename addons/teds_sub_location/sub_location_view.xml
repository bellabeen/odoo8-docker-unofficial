<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>


<record model="ir.ui.view" id="teds_sub_location_tree_view">
    <field name="name">teds.sub.location.tree</field>
    <field name="model">teds.sub.location</field>
    <field name="arch" type="xml">
        <tree string="List Sub Location">
             
            <field name="name"/>
            <field name="branch_id"/>
            <field name="product_id"/>
            <field name="priority"/>
        </tree>
     </field>
</record>

<record model="ir.ui.view" id="teds_sub_location_form_view">
    <field name="name">teds.sub.location.form</field>
    <field name="model">teds.sub.location</field>
    <field name="arch" type="xml">
        <form string="Form Sub Location" version="7.0">
            <header>
                    
            <button name="print_dua" string="Print" type="object" class="oe_highlight"/>                    
                   
            </header>
            <sheet>
            <group>
                <field name="name" on_change="to_upper_name(name)"/>
            </group>

            <group col="4" colspan="4">
                <field name="branch_id" options="{'no_open':True,'no_create_edit':True}" />
                <field name="product_id" options="{'no_open':True,'no_create_edit':True}" />
                <field name="priority"/>
            </group>
        </sheet>
        </form>
    </field>
</record>

<record model="ir.ui.view" id="teds_sub_location_search_view">
    <field name="name">teds.sub.location.search</field>
    <field name="model">teds.sub.location</field>
    <field name="arch" type="xml">
        <search string="Search">
            <filter string="Sub Location" icon="terp-partner" domain="[('name', '=', uid)]"/>
            <field name="name"/>
            <field name="branch_id"/>
            <field name="product_id"/>
            <field name="priority"/>
            <group string="Group By...">
                <filter string="Koordinator" icon="terp-personal" context="{'group_by':'name'}"/>
            </group>
        </search>
    </field>
</record>




<record model="ir.actions.act_window" id="sub_location_action">
    <field name="name">Sub Location</field>
    <field name="res_model">teds.sub.location</field>
    <field name="view_type">form</field>
    <field name="view_mode">tree,form</field>
</record>




<menuitem id="teds_sub_lokasi" name="Sub Location" sequence="1" parent="wtc_dealer_menu.workshop_configuration_menu" action="sub_location_action"/>

  <report 
        string="Location" 
        id="menu_sub_location" 
        model="teds.sub.location" 
        report_type="qweb-pdf"
        name="teds_sub_location.report_location" 
        file="teds_sub_location.report_location" 
        menu="False"/>

</data>
</openerp>