			<field name="implied_ids" eval="[(4, ref('group_purchase_requisition_read'))]"/>            	            	
        </record>
        
		<record id="access_purchase_requisition_delete" model="ir.model.access">
            <field name="model_id" ref="model_purchase_requisition"/>
