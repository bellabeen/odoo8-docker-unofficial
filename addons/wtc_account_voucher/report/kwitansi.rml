<story>
<para style="header">[[repeatIn(objects, 'p')]]</para>
			<blockTable colWidths="12cm,7cm" style="Table_No_Line">
	      	   		<tr>
	      	   			<td><para style="terp_header_besar"><u></u> </para></td>
	      	   			<td>
	      	   			<para style="terp_isi_table"> <b>[[ p.branch_id.name]]</b></para>
	      	   			<para style="terp_isi_table"> [[ p.branch_id.street]] </para>
	      	   			<para style="terp_isi_table">[[ p.branch_id.state_id.name]] </para>
	      	   			</td>
	      	   		</tr>
				</blockTable>


	

<nextFrame name="frame_content"/>
<blockTable colWidths="18.5cm" style="Table_No_Line">
   		<tr>
	   		<td>
	   			<blockTable colWidths="4cm,0.5cm,14cm" style="Table_No_Line">
	   			<tr>
				   		<td>No Transaksi</td>
				   		<td>:</td>


				   		<td>[[ p.move_id.ref]]</td>

			   		</tr>
			   		<tr>
				   		<td>No Kwitansi</td>
				   		<td>:</td>


				   		<td>[[ p.kwitansi_id.name]]</td>

			   		</tr>
			   		<tr>
				   		<td>Sudah terima dari</td>
				   		<td>:</td>
				   		<td>[[ p.partner_id.name]]</td>
			   		</tr>
			   		<tr>
				   		<td>Banyak uang</td>
				   		<td>:</td>
				   		<td>[[formatLang(p.amount, digits=0)]]</td>
			   		</tr>
			   		<tr>
				   		<td>Terbilang</td>
				   		<td>:</td>
				   		<td>## [[terbilang(p.amount)]]##</td>

			   		</tr>
			   		<tr>
				   		<td>Untuk Pembayaran</td>
				   		<td>:</td>
				   		<td><para style="terp_isi_table">[[p.name]]</para></td>
			   		</tr>
			   		
				</blockTable>	
	   		</td>
   		</tr>
	</blockTable>	
	<blockTable colWidths="18.5cm" style="Table_No_Line">
		<tr>
			<td><para style="terp_isi_table">[[ p.branch_id.state_id.name]], [[ formatLang(p.date, date= True) ]]</para></td>
		</tr>
		
	</blockTable>
	<para style="terp_enter_tanda_tangan"><font color="white">.</font></para>
	
	<blockTable colWidths="20cm" style="Table_No_Line">
		<tr>
			<td><para style="terp_isi_table"></para></td>
		</tr>
	</blockTable>
	<para style="terp_enter_tanda_tangan"><font color="white">.</font></para>
	<blockTable colWidths="18.5cm" style="Table_No_Line">
		<tr>
			<td>
			<para style="terp_isi_table">[[ user.name]] [[waktu_local()]]</para>
			<para style="terp_isi_table">Putih:Konsumen;Kuning:Fianace;Hijau:Accounting;Biru:Finance Company (Leasing) ;Merah:Kasir</para>
			</td>
		</tr>
	</blockTable>

</story>






