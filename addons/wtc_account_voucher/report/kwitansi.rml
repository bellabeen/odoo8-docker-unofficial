	<frame id="frame_header" x1="0.0" y1="27cm" width="20cm" height="2.6cm"/>
	<frame id="frame_content" x1="0.0" y1="16.8cm" width="20cm" height="10.3cm"/>
	<frame id="frame_footer" x1="0.0" y1="11.3cm" width="20cm" height="0.5cm"/>

	
	
	
   

</pageTemplate>
</template>
