	</blockTable>
	<section>
      <para style="terp_default">[[ repeatIn(p.line_dr_ids, 'x') ]]</para>
      
	<blockTable colWidths="1cm,5cm,5.5cm,2.5cm,2.5cm,3cm" style="Table_Line">
   		<tr>
   			<td><para style="terp_isi_table_line_right"><seq id="number"/> </para></td>
   			<td><para style="terp_isi_table_line">[[ x.move_line_id.name]] </para></td>
   			<td><para style="terp_isi_table_line">[[ x.account_id.name]] </para></td>
   			<td><para style="terp_isi_table_line">[[ formatLang(x.date_original, date= True) ]] </para></td>
   			<td><para style="terp_isi_table_line">[[ formatLang(x.date_due, date= True) ]]</para></td>
   			<td><para style="terp_isi_table_line_right">[[formatLang(x.amount, digits=0)]] </para></td>
   	
   		</tr>
	</blockTable>	
	</section>
	<para><seqReset id="number"/></para>
	
	<blockTable colWidths="1cm,13cm,5.5cm" style="Table_Line">
   		<tr>
   			<td><para style="terp_isi_table_line"> </para></td>
   			<td><para style="terp_isi_table_line">TOTAL DEBIT </para></td>
   			<td><para style="terp_isi_table_line_right">[[formatLang(total_debit(), digits=0)]] </para></td>
   
   		</tr>
	</blockTable>
	
	
	<para style="enter"><font color="white">1</font></para>
	






	<blockTable colWidths="1cm,5cm,5.5cm,2.5cm,2.5cm,3cm" style="Table_Line">
   		<tr>
   			<td><para style="terp_isi_table_line">No. </para></td>
   			<td><para style="terp_isi_table_line">No Bukti </para></td>
   			<td><para style="terp_isi_table_line">No Inv Supplier </para></td>
   			<td><para style="terp_isi_table_line">Tgl Inv </para></td>
   			<td><para style="terp_isi_table_line_center">Tgl Jtp </para></td>
   			<td><para style="terp_isi_table_line_center">Jumlah </para></td>

   		</tr>
	</blockTable>
	
	<section>
      <para style="terp_default">[[ repeatIn(p.line_cr_ids, 'y') ]]</para>
	<blockTable colWidths="1cm,5cm,5.5cm,2.5cm,2.5cm,3cm" style="Table_Line">
   		<tr>
   			<td><para style="terp_isi_table_line_right"><seq id="number"/> </para></td>
   			<td><para style="terp_isi_table_line">[[ y.move_line_id.name]] </para></td>
   			<td><para style="terp_isi_table_line">[[ y.account_id.name]] </para></td>
   			<td><para style="terp_isi_table_line">[[ formatLang(y.date_original, date= True) ]] </para></td>
   			<td><para style="terp_isi_table_line">[[ formatLang(y.date_due, date= True) ]]</para></td>
   			<td><para style="terp_isi_table_line_right">[[formatLang(y.amount, digits=0)]] </para></td>
   	
   		</tr>
	</blockTable>	
	</section>
	<para><seqReset id="number"/></para>
	
	
	
	<blockTable colWidths="1cm,13cm,5.5cm" style="Table_Line">
   		<tr>
   			<td><para style="terp_isi_table_line"> </para></td>
   			<td><para style="terp_isi_table_line">TOTAL  </para></td>
   			<td><para style="terp_isi_table_line_right">[[formatLang(total_credit(), digits=0)]] </para></td>
   
   		</tr>
	</blockTable>
	
	<blockTable colWidths="5cm,4cm,6.5cm,3cm" style="Table_No_Line">
   		<tr>
   			<td><para style="terp_isi_table_line"> Approved by,</para></td>
   			<td><para style="terp_isi_table_line">Finance,</para></td>
   			<td><para style="terp_isi_table_line">Posting, </para></td>
   			<td><para style="terp_isi_table_line">Penerima </para></td>
   		</tr>
	</blockTable>  
	<para style="terp_enter_tanda_tangan"><font color="white">1</font></para>
	
	<blockTable colWidths="5cm,4cm,6.5cm,3cm" style="Table_No_Line">
   		<tr>
   			<td><para style="terp_isi_table_line">[[p.approval_ids.pelaksana_id.name]] </para></td>
   			<td><para style="terp_isi_table_line">[[p.create_uid.name]]</para></td>
   			<td><para style="terp_isi_table_line"> </para></td>
   			<td><para style="terp_isi_table_line"> </para></td>
   		</tr>
	</blockTable>  
	<blockTable colWidths="18.5cm" style="Table_No_Line">
   		<tr>
   			<td><para style="terp_isi_table">Lembar ke-1 Finance / Lembar ke-2 Accounting   [[ user.name]] [[waktu_local()]]</para></td>
   		</tr>
	</blockTable>
  

</story>






