<story>



	


<para style="header">[[repeatIn(objects, 'p')]]</para>
	<para style="header">[[repeatIn(objects, 'p')]] <pageCount/></para>
	<blockTable colWidths="11cm,7.5cm" style="Table_No_Line">
	
   		<tr>
   			<td>
   				<para style="terp_header_besar">[[ p.branch_id.company_id.name]] Head Office </para>
   				<blockTable colWidths="13cm" style="Table_No_Line">
	      	   		<tr>
	      	   			<td><para style="terp_header_kecil">[[ p.branch_id.street]] </para></td>
	      	   		</tr>
	      	   		<tr>
	      	   			<td><para style="terp_header_kecil">[[ p.branch_id.state_id.name]] </para></td>
	      	   		</tr>
				</blockTable>
   			</td>
   			<td>
   			<para style="terp_header_besar">BUKTI PEMBAYARAN</para>
   			<blockTable colWidths="2cm,0.5cm,5cm" style="Table_No_Line">
	      	   		<tr>
	      	   			<td><para style="terp_header_kecil">No </para></td>
	      	   			<td><para style="terp_header_kecil">: </para></td>
	      	   			<td><para style="terp_header_kecil">[[ p.number]] </para></td>
	      	   		</tr>
	      	   		<tr>
	      	   			<td><para style="terp_header_kecil">Tanggal</para></td>
	      	   			<td><para style="terp_header_kecil">: </para></td>
	      	   			<td><para style="terp_header_kecil">[[ formatLang(p.date, date= True) ]]</para></td>
	      	   		</tr>
				</blockTable>
   			
   			</td>
   		</tr>
	</blockTable>
	
	<blockTable colWidths="18.5cm" style="Table_No_Line">
   		<tr>
   			<td>
   				<blockTable colWidths="11.5cm,7cm" style="Table_No_Line">
	      	   		<tr>
	      	   			<td>
	      	   			
	      	   				<blockTable colWidths="2cm,0.5cm,9cm" style="Table_No_Line">
				      	   		<tr>
				      	   			<td><para style="terp_isi_table">Cabang </para></td>
				      	   			<td><para style="terp_isi_table">: </para></td>
				      	   			<td><para style="terp_isi_table">[[ p.branch_id.name]]</para></td>
				      	   		</tr>
				      	   		<tr>
				      	   			<td><para style="terp_isi_table">Supplier </para></td>
				      	   			<td><para style="terp_isi_table">: </para></td>
				      	   			<td><para style="terp_isi_table"> 
				      	   			[[ len(p.partner_id.name) > 100 and p.partner_id.name[:100] + ' ...' or p.partner_id.name ]]
				      	   			[-[[ p.partner_id.rel_code]]</para>
				      	   			
				      	   			</td>
				      	   		</tr>
				      	   		<tr>
				      	   			<td><para style="terp_isi_table"> </para></td>
				      	   			<td><para style="terp_isi_table"> </para></td>
				      	   			<td><para style="terp_isi_table">[[ p.partner_id.street]] </para></td>
				      	   		</tr>
				      	   		<tr>
				      	   			<td><para style="terp_isi_table">Bank </para></td>
				      	   			<td><para style="terp_isi_table">: </para></td>
				      	   			<td><para style="terp_isi_table">[[ p.journal_id.name]]</para></td>
				      	   		</tr>
				      	   		
				      	   		<tr>
				      	   			<td><para style="terp_isi_table">Total </para></td>
				      	   			<td><para style="terp_isi_table">: </para></td>
				      	   			<td><para style="terp_isi_table"> [[formatLang(p.amount, digits=0)]]</para></td>
				      	   		</tr>
							</blockTable>
	      	   			
	      	   			
	      	   			</td>
	      	   			<td>

	      	   				<blockTable colWidths="2cm,0.5cm,4.5cm" style="Table_No_Line">
				      	   		<tr>
				      	   			<td><para style="terp_isi_table">Payment Ref </para></td>
				      	   			<td><para style="terp_isi_table">: </para></td>
				      	   			<td><para style="terp_isi_table">[[ p.reference]]</para></td>
				      	   		</tr>
				      	   		<tr>
				      	   			<td><para style="terp_isi_table">Memo </para></td>
				      	   			<td><para style="terp_isi_table">: </para></td>
				      	   			<td><para style="terp_isi_table">[[ p.name]] </para></td>
				      	   		</tr>
				      	   		<tr>
				      	   			<td><para style="terp_isi_table">Selisih </para></td>
				      	   			<td><para style="terp_isi_table"> :</para></td>
				      	   			<td><para style="terp_isi_table">[[ p.writeoff_amount]] </para></td>
				      	   		</tr>
				      	   		<tr>
				      	   			<td><para style="terp_isi_table">Keterangan </para></td>
				      	   			<td><para style="terp_isi_table">: </para></td>
				      	   			<td><para style="terp_isi_table">[[ p.narration]] </para></td>
				      	   		</tr>
							</blockTable>
	      	   			
	      	   			
	      	   			</td>
	      	   		</tr>
	      	   		
				</blockTable>
   			</td>
   		</tr>
	</blockTable>
	<blockTable colWidths="2cm,0.5cm,16cm" style="Table_No_Line">
   		<tr>
