<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>
<template id="teds_dso_cancel_print_pdf">
    <t t-call="report.html_container">
        <t t-foreach="docs" t-as="o">
            <div class="page">
                <div class="oe_structure"/>
                    <div class="row"> 
                    </div>
                    <div class="col-xs-12" align="center">
                        <p><strong>DEALER SALES ORDER CANCEL</strong></p>
                        <br/>
                    </div>                  
                    <div class="col-xs-8" style="font-size:10px">
                        <table class="table table-bordered">
                            <tr>
                                <th>Branch</th>
                                <th>:</th>
                                <td><span t-esc="data['branch_id']"/></td>
                            </tr>
                            <tr>
                                <th>Division</th>
                                <th>:</th>
                                <td><span t-esc="data['division']"/></td>
                            </tr>
                            <tr>
                                <th>Date</th>
                                <th>:</th>
                                <td><span t-esc="data['date']"/></td>
                            </tr>
                            <tr>
                                <th>Dealer Sale Order</th>
                                <th>:</th>
                                <td><span t-esc="data['dso']"/></td>
                            </tr>
                            <tr>
                                <th>Nama Konsumen</th>
                                <th>:</th>
                                <td><span t-esc="data['partner']"/></td>
                            </tr>
                            <tr>
                                <th>Reason</th>
                                <th>:</th>
                                <td><span t-esc="data['reason']"/></td>
                            </tr>
                            <tr>
                                <th>No Mesin</th>
                                <th>:</th>
                                <td><span t-esc="data['no_mesin']"/></td>
                            </tr>
                            <tr>
                                <th>Optional Denda</th>
                                <th>:</th>
                                <td>[ ] Tanpa Denda</td>
                            </tr>
                            <tr>
                                <th colspan="2"></th>
                                <td>[ ] Dengan denda , dengan no bukti : AR/D..................</td>
                            </tr>
                            <tr>
                                <th>No PV (Mediator)</th>
                                <th>:</th>
                                <td></td>
                            </tr>
                        </table>
                    </div>
                    <div class="col-xs-12" style="font-size:10px">
                        <br/>
                        <table class="table table-bordered">
                            <tr>
                                <td align="center">Diajukan Oleh</td>
                                <td align="center">Mengetahui</td>
                                <td colspan="3" align="center">Disetujui Oleh</td>
                            </tr>
                            <tr>
                                <td height="100"/>
                                <td height="100"/>
                                <td height="100"/>
                                <td height="100"/>
                                <td height="100"/>
                            </tr>
                            <tr>
                                <td height="30"/>
                                <td height="30"/>
                                <td height="30"/>
                                <td height="30"/>
                                <td height="30"/>
                            </tr>
                            <tr>
                                <td align="center" width="20%">ADH</td>
                                <td align="center" width="20%">SOH</td>
                                <td align="center" width="20%">Area Manager</td>
                                <td align="center" width="20%">Admin Manager</td>
                                <td align="center" width="20%">GM</td>
                            </tr>
                        </table>
                        <p><span t-esc="data['today']"/>, <span t-esc="data['user']"/></p>
                    </div>

                <div class="oe_structure"/>
            </div>
        </t>
    </t>
</template>
</data>
</openerp>
