<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    
    <!-- DSO Cancellation -->
    	
    	<record model="ir.ui.view" id="dealer_sales_order_cancel_tree_view">
            <field name="name">dealer.sales.order.cancel.tree</field>
            <field name="model">dealer.sales.order.cancel</field>
            <field name="arch" type="xml">
                <tree string="Dealer Sales Order Cancel">
                	<field name="name"/>
                    <field name="branch_id"/>
                    <field name="division"/>
                	<field name="dealer_sales_order_id"/>
                    <field name="date"/>
                    <field name="state"/>
				</tree>
			</field>
        </record>
        
        <record id="view_dealer_sales_order_cancel_search" model="ir.ui.view">
            <field name="name">dealer.sales.order.cancel.search</field>
            <field name="model">dealer.sales.order.cancel</field>
            <field name="arch" type="xml">
                <search string="Search Dealer Sales Order Cancel">
                    <field name="name"/>
                    <field name='branch_id'/>
                    <field name='division'/>
                    <field name='dealer_sales_order_id'/>
                    <field name='date'/>
                    <field name='state'/>
                    <group expand="0" string="Group By">
                        <filter string="Branch" icon="terp-go-month" domain="[]" context="{'group_by':'branch_id'}"/>
                    </group>
                </search>
            </field>
        </record>
        
        <record model="ir.ui.view" id="dealer_sales_order_cancel_form_view">
            <field name="name">dealer.sales.order.cancel.form</field>
            <field name="model">dealer.sales.order.cancel</field>
            <field name="arch" type="xml">
                
                <form string="Dealer Sales Order Cancel">
                    <header>
                        <button name="request_approval" string="RFA" states="draft" type="object" class="oe_highlight" groups="wtc_cancellation.group_dealer_sales_order_cancel_rfa"/>
                        <button name="approve" string="Approve" states="waiting_for_approval" type="object" class="oe_highlight"/>
                        <button name="%(wtc_approval.wtc_approval_reject_wizard_action)d" attrs="{'invisible': [('state','!=','waiting_for_approval')]}" string="Reject" type="action" context="{'model_name':'dealer.sales.order.cancel','update_value':{'approval_state':'r','state':'draft'}}"/>
                        <button name="confirm" string="Confirm" states="approved" type="object" class="oe_highlight" groups="wtc_cancellation.group_dealer_sales_order_cancel_confirm"/>
                        <button name="%(wtc_approval.wtc_approval_cancel_wizard_action)d" attrs="{'invisible': [('state','!=','waiting_for_approval')]}" string="Cancel Approval" type="action" context="{'model_name':'dealer.sales.order.cancel','update_value':{'approval_state':'b','state':'draft'}}" groups="wtc_cancellation.group_dealer_sales_order_cancel_cancel_approval"/>
                        <button name="action_print_form" string="Print Form" type="object" states="draft"/>
                        <field name="state" widget="statusbar" statusbar_visible="draft,waiting_for_approval,approved,confirmed"/>
                    </header>
                    <sheet>
                    	<div class="oe_title">                      
		       				<h1>
					 			<field name="name" class="oe_inline" readonly="1"/>	
		    			    </h1>
	                    </div>
                    	<group>
                            <group>
                                <field name="branch_id" options="{'no_create':True,'no_open':True}" required="True" attrs="{'readonly':[('state','!=','draft')]}"/>
                                <field name="dealer_sales_order_id" required="True" options="{'no_create':True,'no_open':True}" domain="[('state','in',('progress','done')),('branch_id','=',branch_id),('division','=',division)]" attrs="{'readonly':[('state','!=','draft')]}"/>
                                <field name="customer_id" options="{'no_open':True,'no_create':True}"/>
                                <field name="approval_state"/>
                            </group>
                            <group>
                                <field name="division" required="True" attrs="{'readonly':[('state','!=','draft')]}"/>
                                <field name="date" readonly="True"/>
                                <field name="reason" required="True" attrs="{'readonly':[('state','!=','draft')]}"/>
                            </group>
                        </group>
                        <notebook>
                        	<page string="Approval">
								<field name="approval_ids">
									<tree string="Approval" editable="bottom" create="false" edit="false" delete="false" >
										<field name="group_id"/>
										<field name="branch_id"/>
										<field name="division"/>
										<field name="limit"/>
										<field name="sts"/>
										<field name="reason"/>
										<field name="pelaksana_id"/>
										<field name="tanggal"/>
									</tree>
								</field>
							</page>
							<page string="Journal Items" states="confirmed">
								<group col="4">
	                                <field name="period_id" readonly="1"/>
	                            </group>
								<field name="move_ids" readonly="1">
	                               <tree string="Journal Items">
	                                   <field name="move_id"/>
	                                   <field name="branch_id"/>
	                                   <field name="ref"/>
	                                   <field name="date"/>
	                                   <field name="statement_id"/>
	                                   <field name="partner_id"/>
	                                   <field name="account_id"/>
	                                   <field name="name"/>
	                                   <field name="debit"/>
	                                   <field name="credit"/>
	                                   <field name="state"/>
	                                   <field name="reconcile_id"/>
	                               </tree>
	                            </field>
							</page>
							<page string="Audit Trail">
								<group>
									<group>
										<field name="create_uid" readonly="1"></field>
										<field name="create_date" readonly="1"></field>
										<field name="confirm_uid" readonly="1"></field>
										<field name="confirm_date" readonly="1"></field>
									</group>
									<group>
										<field name="write_uid" readonly="1"></field>
										<field name="write_date" readonly="1"></field>
									</group>
								</group>
							</page>
                        </notebook>
                    </sheet>
                </form>
                
            </field>
        </record>
        
