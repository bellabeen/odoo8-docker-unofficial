<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
    
		<record id="wtc_branch_config_stock_view" model="ir.ui.view">
            <field name="name">wtc.branch.config.cancel.view</field>
            <field name="model">wtc.branch.config</field>
            <field name="inherit_id" ref="wtc_branch.wtc_branch_config_form_view"/>
            <field name="arch" type="xml">
            
                <xpath expr="/form/sheet/group" position="after">
               		<group string="Pembatalan">
               			<field name="dso_cancel_journal_id" options="{'no_create': True,'no_open':True}"/>
               			<field name="payment_cancel_journal_id" options="{'no_create': True,'no_open':True}"/>
               			<field name="so_cancel_unit_journal_id" options="{'no_create': True,'no_open':True}"/>
               			<field name="so_cancel_sparepart_journal_id" options="{'no_create': True,'no_open':True}"/>
                   	</group>
                </xpath>
                
            </field>
        </record>
        
	</data>
</openerp>