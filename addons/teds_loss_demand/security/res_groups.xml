<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">  	
    	
        <record id="group_teds_loss_demand_read" model="res.groups">
           <field name="category_id" ref="wtc_branch.tdm_role"/>
           <field name="name">Teds Loss Demand Read</field> 
           <field name="menu_access" eval="[(4, ref('teds_loss_demnad_menuitem'))]"/>  
        </record> 
        <record id="access_teds_loss_demand_read" model="ir.model.access">
           <field name="model_id" ref="model_teds_loss_demand"/>
           <field name="name">teds_loss_demand read</field> 
           <field name="group_id" ref="group_teds_loss_demand_read"/>  
           <field name="perm_read">1</field> 
        </record> 

                                
        <record id="group_teds_loss_demand_update" model="res.groups">
           <field name="category_id" ref="wtc_branch.tdm_role"/>
           <field name="name">Teds Loss Demand Update</field> 
           <field name="menu_access" eval="[(4, ref('teds_loss_demnad_menuitem'))]"/>  
		       <field name="implied_ids" eval="[(4, ref('group_teds_loss_demand_read'))]"/>
        </record>         
        <record id="access_teds_loss_demand_update" model="ir.model.access">
           <field name="model_id" ref="model_teds_loss_demand"/>
           <field name="name">teds_loss_demand update</field> 
           <field name="group_id" ref="group_teds_loss_demand_update"/>  
           <field name="perm_create">1</field> 
           <field name="perm_write">1</field> 
        </record> 

                  
        <record id="group_teds_loss_demand_delete" model="res.groups">
           <field name="category_id" ref="wtc_branch.tdm_role"/>
           <field name="name">Teds Loss Demand Delete</field> 
           <field name="menu_access" eval="[(4, ref('teds_loss_demnad_menuitem'))]"/>   
		       <field name="implied_ids" eval="[(4, ref('group_teds_loss_demand_read'))]"/>        	
        </record>            
        <record id="access_teds_loss_demand_delete" model="ir.model.access">
           <field name="model_id" ref="model_teds_loss_demand"/>
           <field name="name">teds_loss_demand delete</field> 
           <field name="group_id" ref="group_teds_loss_demand_delete"/>  
           <field name="perm_unlink">1</field> 
        </record>      

 

<!-- Laporan Demand -->

      <record id="group_teds_report_demand_read" model="res.groups">
           <field name="category_id" ref="wtc_branch.tdm_role"/>
           <field name="name">Teds Laporan Loss Demand Read</field> 
           <field name="menu_access" eval="[(4, ref('menu_demand_report'))]"/>  
        </record> 
        <record id="access_teds_report_demand_read" model="ir.model.access">
           <field name="model_id" ref="model_teds_report_demand"/>
           <field name="name">teds_report_demand read</field> 
           <field name="group_id" ref="group_teds_report_demand_read"/>  
           <field name="perm_read">1</field> 
        </record> 




      <record id="wtc_branch.group_sys_admin" model="res.groups">
    			<field name="implied_ids" eval="[(4,ref('group_teds_loss_demand_read')),
    												(4,ref('group_teds_loss_demand_update')),
                            (4,ref('group_teds_loss_demand_delete')),
                            (4,ref('group_teds_report_demand_read')),
    											]"/> 
		  </record>           
                                                  
    </data>
</openerp>
  