<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
		<record id="view_teds_laporan_stock_opname_wizard" model="ir.ui.view">
			<field name="name">teds.laporan.stock.opname.wizard</field>
			<field name="model">teds.laporan.stock.opname.wizard</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Laporan Stock Opname">
					<group attrs="{'invisible':[('state_x','=','get')]}">
						<group>
							<field name="options" required="1"/>
							<field name="status" required="1"/>
							<label for="periode" string="Periode"/>
							<div>
								<field name="periode" class="oe_inline" required="1"/>
								<field name="tahun" class="oe_inline" placeholder="Tahun" required="1"/>
							</div>
						</group>
					</group>
					<notebook attrs="{'invisible':[('state_x','=','get')]}">
						<page string="Branches">
							<field name="branch_ids" colspan="4" nolabel="1"/>
						</page>
					</notebook>

                	<field invisible="1" name="state_x"/>                
                	<newline/>    
                	<group col="4" fill="0" attrs="{'invisible':[('state_x','!=','get')]}">
	                    <separator colspan="4" string="Excel Export Generated. Click link below to download ..." attrs="{'invisible':[('state_x','=','choose')]}"/>
	                    <field name="name" invisible="1" width="100" attrs="{'invisible':[('state_x','=','choose')]}"/>
	                    <field name="data_x" nolabel="1" readonly="0" width="100" filename="name" attrs="{'invisible':[('state_x','=','choose')]}"/>
	                </group> 
	                <footer>
	                    <button name="excel_report" string="Export Data" type="object" class="oe_highlight" attrs="{'invisible':[('state_x','=','get')]}"/>
	                    <label string=" or " attrs="{'invisible':[('state_x','=','get')]}"/>
	                    <button string="Cancel" special="cancel" class="oe_link" attrs="{'invisible':[('state_x','=','get')]}"/>
	                </footer>
				</form>
			</field>
		</record>

		<record id="action_teds_laporan_stock_opname_wizard" model="ir.actions.act_window">
			<field name="name">Stock Opname</field> 
			<field name="res_model">teds.laporan.stock.opname.wizard</field>
			<field name="view_type">form</field>
			<field name="view_mode">form</field>
	     	<field name="view_id" ref="view_teds_laporan_stock_opname_wizard"/>
			<field name="target">new</field>
		</record>

		<menuitem id="teds_laporan_stock_opname_wizard_menuitem" parent="stock_opname_report_sub_menu" action="action_teds_laporan_stock_opname_wizard" sequence="1"/>

		<!--  Laporan Data SO -->
		<record id="view_teds_laporan_stock_opname_detail_wizard" model="ir.ui.view">
			<field name="name">teds.laporan.stock.opname.detail.wizard</field>
			<field name="model">teds.laporan.stock.opname.wizard</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Laporan Stock Opname Detail">
					<group attrs="{'invisible':[('state_x','=','get')]}">
						<group>
							<field name="options" required="1"/>
							<field name="status" required="1"/>
							<label for="periode" string="Periode"/>
							<div>
								<field name="periode" class="oe_inline" required="1"/>
								<field name="tahun" class="oe_inline" placeholder="Tahun" required="1"/>
							</div>
						</group>
					</group>
					<notebook attrs="{'invisible':[('state_x','=','get')]}">
						<page string="Branches">
							<field name="branch_ids" colspan="4" nolabel="1"/>
						</page>
					</notebook>

                	<field invisible="1" name="state_x"/>                
                	<newline/>    
                	<group col="4" fill="0" attrs="{'invisible':[('state_x','!=','get')]}">
	                    <separator colspan="4" string="Excel Export Generated. Click link below to download ..." attrs="{'invisible':[('state_x','=','choose')]}"/>
	                    <field name="name" invisible="1" width="100" attrs="{'invisible':[('state_x','=','choose')]}"/>
	                    <field name="data_x" nolabel="1" readonly="0" width="100" filename="name" attrs="{'invisible':[('state_x','=','choose')]}"/>
	                </group> 
	                <footer>
	                    <button name="excel_report_detail" string="Export Data" type="object" class="oe_highlight" attrs="{'invisible':[('state_x','=','get')]}"/>
	                    <label string=" or " attrs="{'invisible':[('state_x','=','get')]}"/>
	                    <button string="Cancel" special="cancel" class="oe_link" attrs="{'invisible':[('state_x','=','get')]}"/>
	                </footer>
				</form>
			</field>
		</record>

		<record id="action_teds_laporan_stock_opname_detail_wizard" model="ir.actions.act_window">
			<field name="name">Stock Opname (Detail)</field> 
			<field name="res_model">teds.laporan.stock.opname.wizard</field>
			<field name="view_type">form</field>
			<field name="view_mode">form</field>
			<field name="view_id" ref="view_teds_laporan_stock_opname_detail_wizard"/>
			<field name="target">new</field>
		</record>

		<menuitem id="teds_laporan_stock_opname_detail_wizard_menuitem" parent="stock_opname_report_sub_menu" action="action_teds_laporan_stock_opname_detail_wizard" sequence="2"/>
	
	</data>
</openerp>