<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>
<template id="teds_stock_opname_stnk_print_bakso">
    <t t-call="report.html_container">
        <t t-foreach="docs" t-as="o">
                <div class="page">
                    <div class="oe_structure"/>
                        <div class="row"> 
                        </div>
                        <table class="table table-bordered">
                            <tr>
                                <td><span align="center"><h3>BERITA ACARA STOCK OPNAME STNK</h3></span></td>
                            </tr>
                            <tr>
                                <th>No Refrensi: <span t-esc="name(data)"/></th>
                            </tr>
                            <tr>
                                <td>
                                    <table class="table">
                                        <tr>
                                            <th>Branch</th>
                                            <th>: <span t-esc="branch(data)"/></th>
                                            <th>Jam Mulai</th>
                                            <th>: <span t-esc="jam_mulai(data)"/></th>
                                        </tr>
                                        <tr>
                                            <th>Division</th>
                                            <th>: <span t-esc="division(data)"/></th>
                                            <th>Jam Selesai</th> 
                                            <th>: <span t-esc="jam_selesai(data)"/></th>
                                        </tr>
                                        <tr>
                                            <th>Tanggal SO</th>
                                            <th colspan="3">: <span t-esc="tgl_so(data)"/></th>
                                        </tr>
                                    </table>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <table width="100%" style="font-size:12px">
                                        <tr height="20px">
                                            <th style="font-size:12">Hasil Stock Opname hari ini sebagai berikut:</th>
                                        </tr>
                                        <tr>
                                            <table class="table table-bordered" style="font-size:10px">
                                                <t t-if="saldo_sistem(data) != 0">
                                                    <tr>
                                                        <th>Saldo STNK by Sistem</th>
                                                        <td><span t-esc="saldo_sistem(data)"/></td>
                                                    </tr>
                                                </t>
                                                <t t-if="saldo_cabang(data) != 0">
                                                    <tr>
                                                        <th>Saldo STNK by Fisik di Cabang</th>
                                                        <td><span t-esc="saldo_cabang(data)"/></td>
                                                    </tr>
                                                </t>
                                                <t t-if="saldo_ho(data) != 0">
                                                    <tr>
                                                        <th>Saldo STNK by Fisik di HO Pasar Minggu</th>
                                                        <td><span t-esc="saldo_ho(data)"/></td>
                                                    </tr>
                                                </t>
                                                <t t-if="saldo_md(data) != 0">
                                                    <tr>
                                                        <th>Saldo STNK by Fisik di Main Dealer</th>
                                                        <td><span t-esc="saldo_md(data)"/></td>
                                                    </tr>
                                                </t>
                                                <t t-if="saldo_birojasa(data) != 0">
                                                    <tr>
                                                        <th>Saldo STNK by Fisik di Biro Jasa</th>
                                                        <td><span t-esc="saldo_birojasa(data)"/></td>
                                                    </tr>
                                                </t>
                                                <t t-if="saldo_konsumen(data) != 0">
                                                    <tr>
                                                        <th>STNK Sudah Penyerahan ke Konsumen</th>
                                                        <td><span t-esc="saldo_konsumen(data)"/></td>
                                                    </tr>
                                                </t>
                                                <t t-if="saldo_lainnya(data) != 0">
                                                    <tr>
                                                        <th>Hilang / Fisik tidak diketahui</th>
                                                        <td><span t-esc="saldo_lainnya(data)"/></td>
                                                    </tr>
                                                </t>
                                                <t t-if="selisih_sistem_fisik(data) != 0">
                                                    <tr>
                                                        <th>Selisih Sistem vs Fisik</th>
                                                        <td><span t-esc="selisih_sistem_fisik(data)"/></td>
                                                    </tr>
                                                </t>
                                                <t t-if="other_stnk(data) != 0">
                                                    <tr>
                                                        <th>Fisik tidak tercatat sistem</th>
                                                        <td><span t-esc="other_stnk(data)"/></td>
                                                    </tr>
                                                </t>
                                                <t t-if="total_stock(data) != 0">
                                                    <tr>
                                                        <th>Total STNK di Cabang</th>
                                                        <td><span t-esc="total_stock(data)"/></td>
                                                    </tr>
                                                </t>
                                            </table>
                                        </tr>
                                        <tr height="5px">
                                            <th><u>PERNYATAAN</u></th>
                                        </tr>
                                        <tr height="10px">
                                            <td>                                            
                                                Proses stock opname ini telah berjalan dengan baik dan hasil ini telah dinyatakan sah untuk diakui dan telah didukung oleh dokumen yang seharusnya dilampirkan.
                                            </td>
                                        </tr>
                                        <tr height="20px">
                                            <td>
                                                <table class="table table-bordered">
                                                    <tr>
                                                        <td><span t-esc="note_bakso(data)"/></td>
                                                    </tr>
                                                </table>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td align="center">
                                                <table class="table table-bordered" style="font-size:10px">
                                                    <tr>
                                                        <td colspan="3" align="center"><b>PERSON IN CHARGE YANG BERTUGAS</b></td>
                                                    </tr>
                                                    <tr>
                                                        <td align="center">PIC SO</td>
                                                        <td colspan="2" align="center">MENGETAHUI</td>
                                                    </tr>
                                                    <tr height="80px">
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                    </tr>
                                                    <tr>
                                                        <th><span t-esc="adh(data)"/></th>
                                                        <th><span t-esc="staff_bbn(data)"/></th>
                                                        <th><span t-esc="soh(data)"/></th>
                                                    </tr>
                                                    <tr>
                                                        <td>ADH</td>
                                                        <td>Staff BBN</td>
                                                        <td>SOH</td>  
                                                    </tr>
                                                    <tr>
                                                        <td colspan="3">
                                                            <span t-esc="data['user']"/>, <span t-esc="data['date']"/>
                                                        </td>
                                                    </tr>
                                                </table>
                                            </td>
                                        </tr>  
                                    </table>
                                </td>
                            </tr>                            
                        </table>
                    <div class="oe_structure"/>
                </div>
        </t>
    </t>
</template>
</data>
</openerp>
