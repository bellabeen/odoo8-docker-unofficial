<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
   
<!-- Monitoring Update LOT  -->
		
        <record id="group_button_b2b_file_monitoring_update_lot" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">B2B File Monitoring Button Update lot</field>
            <field name="implied_ids" eval="[(4, ref('group_b2b_file_monitoring_read'))]"/>
        </record>
                
        <record id="access_button_b2b_file_monitoring_update_lot_write" model="ir.model.access">
            <field name="model_id" ref="b2b_file.model_b2b_file_monitoring"/>
            <field name="name">access_button_b2b_file_monitoring_update_lot write</field>
            <field name="group_id" ref="group_button_b2b_file_monitoring_update_lot"/>
            <field name="perm_write">1</field>
        </record>

<!-- Monitoring Proses Line  -->
		
        <record id="group_button_b2b_file_monitoring_process_line" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">B2B File Monitoring Button Proses Line</field>
            <field name="implied_ids" eval="[(4, ref('group_b2b_file_monitoring_read'))]"/>
        </record>
                
        <record id="access_button_b2b_file_monitoring_process_line_write" model="ir.model.access">
            <field name="model_id" ref="b2b_file.model_b2b_file_monitoring"/>
            <field name="name">access_button_b2b_file_monitoring_process_line write</field>
            <field name="group_id" ref="group_button_b2b_file_monitoring_process_line"/>
            <field name="perm_write">1</field>
        </record>

<!-- Monitoring Move File  -->
		
        <record id="group_button_b2b_file_monitoring_move_line" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">B2B File Monitoring Button Move File</field>
            <field name="implied_ids" eval="[(4, ref('group_b2b_file_monitoring_read'))]"/>
        </record>
                
        <record id="access_button_b2b_file_monitoring_move_line_write" model="ir.model.access">
            <field name="model_id" ref="b2b_file.model_b2b_file_monitoring"/>
            <field name="name">access_button_b2b_file_monitoring_move_line write</field>
            <field name="group_id" ref="group_button_b2b_file_monitoring_move_line"/>
            <field name="perm_write">1</field>
        </record>
                              
        <record id="wtc_branch.group_sys_admin" model="res.groups">
			<field name="implied_ids" eval="[(4,ref('group_button_b2b_file_monitoring_update_lot')),
				                             (4,ref('group_button_b2b_file_monitoring_process_line')),						
				                             (4,ref('group_button_b2b_file_monitoring_move_line')),						
				                              ]">
			</field> 
		</record>

    </data>
</openerp>
