<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
	
	<record id="view_b2b_configuration_folder_tree" model="ir.ui.view">
       <field name="name">b2b.configuration.folder.tree</field>
       <field name="model">b2b.configuration.folder</field>
       <field name="arch" type="xml">
          <tree string="B2B Configuration Folder">
             <field name="folder_in"/>
             <field name="folder_proses"/>
             <field name="folder_archin"/>
             <field name="folder_error"/>
          </tree>
       </field>
     </record>
     
	<record id="b2b_configuration_folder_form" model="ir.ui.view">
      <field name="name">b2b.configuration.folder.form</field>
      <field name="model">b2b.configuration.folder</field>            
      <field name="arch" type="xml">
         <form string="B2B Configuration File">
           <sheet>
	       		<div class="oe_title">                      
					<h1>
	     				<label string="B2B Configuration Folder"/>
	 				</h1>
	         	</div>
                 <group>
               		<group col="4">
                		 <field name="folder_in"/>
			             <field name="folder_proses"/>
			             <field name="folder_archin"/>
			             <field name="folder_error"/>
			             <field name="active"/>
               		</group>
               	</group>
            </sheet>   	                    
          </form>
       </field>
    </record>
        
    	<record model="ir.actions.act_window" id="b2b_configuration_folder_action">
		    <field name="name">B2B Configuration Folder</field>
		    <field name="res_model">b2b.configuration.folder</field>
		    <field name="view_type">form</field>
		    <field name="view_mode">tree,form</field>
		</record>

       	<menuitem id="b2b_configuration_folder_menuitem"  sequence="1" parent="wtc_dealer_menu.ma_b2b_menu" action="b2b_configuration_folder_action"/>
	</data>
</openerp>