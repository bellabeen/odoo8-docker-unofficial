<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
		<record id="wtc_purchase_order_branch_config_view" model="ir.ui.view">
            <field name="name">wtc.branch.config.purchase.order.view</field>
            <field name="model">wtc.branch.config</field>
            <field name="inherit_id" ref="wtc_branch.wtc_branch_config_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="/form/sheet/group" position="after">
                  
                   		<group string="Purchase Order">
                   			<field name="wtc_po_journal_unit_id" domain="[('type','=','purchase')]"/>
                   			<field name="wtc_po_journal_sparepart_id" domain="[('type','=','purchase')]"/>
                   			<field name="wtc_po_journal_umum_id" domain="[('type','=','purchase')]"/>
                   			<field name="wtc_po_account_discount_cash_id"/>
                   			<field name="wtc_po_account_discount_program_id"/>
                   			<field name="wtc_po_account_discount_lainnya_id"/>
                   			<field name="wtc_po_account_discount_all_workshop_id"/>
                   			<field name="wtc_po_journal_blind_bonus_beli_id" domain="[('type','=','sale')]"/>
                   			<field name="wtc_po_account_blind_bonus_beli_dr_id"/>
                   			<field name="wtc_po_account_blind_bonus_beli_cr_id"/>
                   			<field name="wtc_po_account_blind_bonus_performance_dr_id"/>
                   			<field name="wtc_po_account_blind_bonus_performance_cr_id"/>
                   		</group>
                   
                </xpath>
            </field>
        </record>
</data>
</openerp>