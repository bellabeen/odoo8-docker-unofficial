<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record id="teds_accounting_recurring_form_view" model="ir.ui.view">
			<field name="name">teds.accounting.recurring.form</field>
			<field name="model">account.subscription</field>
			<field name="inherit_id" ref="account.view_subscription_form"/>
			<field name="arch" type="xml">
				<field name="name" position="before">
					<field name="branch_id" options="{'no_create': True,'no_open': True}" required="1" attrs="{'readonly': ['|',('state','!=','draft')]}"/>
				</field>
				<field name="date_start" position="before">
					<field name="division" />
				</field>
			</field>
		</record>
		
	</data>
</openerp>