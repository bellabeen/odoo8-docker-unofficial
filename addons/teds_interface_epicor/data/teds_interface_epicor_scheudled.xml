<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">    
        <record id="ir_cron_teds_interface_epicor_auto_generate_account_move" model="ir.cron">
            <field name="name">Interface Epicor Generate Account Move</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="nextcall" eval="(DateTime.now()).strftime('%Y-%m-%d 01:00:00')" />
            <field eval="False" name="doall"/>
            <field eval="'teds.interface.epicor'" name="model"/>
            <field eval="'generate_account_move'" name="function"/>
            <field eval="'(None,)'" name="args"/>
        </record> 
        
        <record id="ir_cron_teds_interface_epicor_auto_generate_file" model="ir.cron">
            <field name="name">Interface Epicor Generate File</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="nextcall" eval="(DateTime.now()).strftime('%Y-%m-%d 02:00:00')" />
            <field eval="False" name="doall"/>
            <field eval="'teds.interface.epicor'" name="model"/>
            <field eval="'generate_file'" name="function"/>
            <field eval="'(None,)'" name="args"/>
        </record> 

    </data>
</openerp>    