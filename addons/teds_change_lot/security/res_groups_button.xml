<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
                
<!-- Disposal Asset  -->
		
        <record id="group_change_lot_rfa" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">RFA - Change Data Serial Number</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_change_lot_read'))]"/>
        </record>
             
        <record id="group_change_lot_cancel_app" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Cancel - Change Data Serial Number</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_change_lot_read'))]"/>
        </record>
                
        <record id="group_change_lot_confirm" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Confirm - Change Data Serial Number</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_change_lot_read'))]"/>
        </record>
                
        <record id="wtc_branch.group_sys_admin" model="res.groups">
			<field name="implied_ids" eval="[
											(4,ref('group_change_lot_rfa')),
											(4,ref('group_change_lot_cancel_app')),
											(4,ref('group_change_lot_confirm')),
											 ]"/> 
		</record>                           
    </data>
</openerp>
