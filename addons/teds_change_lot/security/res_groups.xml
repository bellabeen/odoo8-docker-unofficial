<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

<!-- Change Data Serial Number -->
		
        <record id="group_teds_change_lot_read" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Change Data Serial Number Read</field>
            <field name="menu_access" eval="[(4, ref('teds_change_lot_submenu'))]"/>
        </record>
        
        <record id="access_teds_change_lot_read" model="ir.model.access">
            <field name="model_id" ref="model_teds_change_lot"/>
            <field name="name">teds_change_lot read</field>
            <field name="group_id" ref="group_teds_change_lot_read"/>
            <field name="perm_read">1</field>
        </record>
		
        <record id="access_teds_change_lot_account_read" model="ir.model.access">
            <field name="model_id" ref="account_asset.model_account_asset_asset"/>
            <field name="name">teds_change_lot_account_asset read</field>
            <field name="group_id" ref="group_teds_change_lot_read"/>
            <field name="perm_read">1</field>
        </record>
        
        <record id="access_teds_change_lot_approval_line_read" model="ir.model.access">
            <field name="model_id" ref="wtc_approval.model_wtc_approval_line"/>
            <field name="name">teds_change_lot_approval_line read</field>
            <field name="group_id" ref="group_teds_change_lot_read"/>  
            <field name="perm_read">1</field>
        </record>
        
        <record id="group_teds_change_lot_update" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Change Data Serial Number Update</field>
            <field name="menu_access" eval="[(4, ref('teds_change_lot_submenu'))]"/>
			<field name="implied_ids" eval="[(4, ref('group_teds_change_lot_read'))]"/>
        </record>

        <record id="access_teds_change_lot_update" model="ir.model.access">
            <field name="model_id" ref="model_teds_change_lot"/>
            <field name="name">teds_change_lot update</field>
            <field name="group_id" ref="group_teds_change_lot_update"/>
            <field name="perm_write">1</field>
        </record>
        
        <record id="access_teds_change_lot_approval_line_update" model="ir.model.access">
            <field name="model_id" ref="wtc_approval.model_wtc_approval_line"/>
            <field name="name">teds_change_lot_approval_line update</field>
            <field name="group_id" ref="group_teds_change_lot_update"/>
            <field name="perm_create">1</field>
            <field name="perm_write">1</field>
        </record>
		
        <record id="group_teds_change_lot_delete" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Change Data Serial Number Delete</field> 
            <field name="menu_access" eval="[(4, ref('teds_change_lot_submenu'))]"/>
			<field name="implied_ids" eval="[(4, ref('group_teds_change_lot_read'))]"/>            	            	
        </record>
        
		<record id="access_teds_change_lot_delete" model="ir.model.access">
            <field name="model_id" ref="model_teds_change_lot"/>
            <field name="name">teds_change_lot delete</field>
            <field name="group_id" ref="group_teds_change_lot_delete"/>
            <field name="perm_unlink">1</field>
        </record>
        		
    
        <record id="wtc_branch.group_sys_admin" model="res.groups">
			<field name="implied_ids" eval="[(4,ref('group_teds_change_lot_read')),	
											(4,ref('group_teds_change_lot_update')),	
											(4,ref('group_teds_change_lot_delete')),					
			]"/> 
		</record>   
		
    </data>
</openerp>
