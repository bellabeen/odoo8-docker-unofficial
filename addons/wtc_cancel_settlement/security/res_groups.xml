<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
    
<!-- Settlement Cancel -->
		
        <record id="group_settlement_cancel_read" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Settlement Cancel Read</field>
            <field name="menu_access" eval="[
				(4, ref('settlement_cancel_menu')),
				]"/>
        </record>
        
        <record id="access_settlement_cancel_read" model="ir.model.access">
            <field name="model_id" ref="model_wtc_settlement_cancel"/>
            <field name="name">settlement_cancel read</field>
            <field name="group_id" ref="group_settlement_cancel_read"/>
            <field name="perm_read">1</field>
        </record>
        
        <record id="access_settlement_cancel_approval_line_read" model="ir.model.access">
            <field name="model_id" ref="wtc_approval.model_wtc_approval_line"/>
            <field name="name">settlement_cancel_approval_line read</field>
            <field name="group_id" ref="group_settlement_cancel_read"/>  
            <field name="perm_read">1</field>
        </record>
        
        <record id="access_settlement_cancel_wo_read" model="ir.model.access">
            <field name="model_id" ref="wtc_advance_payment.model_wtc_settlement"/>
            <field name="name">settlement_cancel_wo read</field>
            <field name="group_id" ref="group_settlement_cancel_read"/>
            <field name="perm_read">1</field>
        </record>
        
        <record id="access_settlement_cancel_settlement_line_read" model="ir.model.access">
            <field name="model_id" ref="wtc_advance_payment.model_wtc_settlement"/>
            <field name="name">settlement_cancel_settlement_line read</field>
            <field name="group_id" ref="group_settlement_cancel_read"/>
            <field name="perm_read">1</field>
        </record>
        
        <record id="group_settlement_cancel_update" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Settlement Cancel Update</field>
            <field name="menu_access" eval="[
				(4, ref('settlement_cancel_menu')),
				]"/>
			<field name="implied_ids" eval="[(4, ref('group_settlement_cancel_read'))]"/>
        </record>
        
        <record id="access_settlement_cancel_update" model="ir.model.access">
            <field name="model_id" ref="model_wtc_settlement_cancel"/>
            <field name="name">settlement_cancel update</field> 
            <field name="group_id" ref="group_settlement_cancel_update"/>  
            <field name="perm_create">1</field>
            <field name="perm_write">1</field>
        </record>
		
        <record id="group_settlement_cancel_delete" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Settlement Cancel Delete</field> 
            <field name="menu_access" eval="[
				(4, ref('settlement_cancel_menu')),
				]"/>
			<field name="implied_ids" eval="[(4, ref('group_settlement_cancel_read'))]"/>            	            	
        </record>
        
		<record id="access_settlement_cancel_delete" model="ir.model.access">
            <field name="model_id" ref="model_wtc_settlement_cancel"/>
            <field name="name">settlement_cancel delete</field>
            <field name="group_id" ref="group_settlement_cancel_delete"/>
            <field name="perm_unlink">1</field>
        </record>
        
        <record id="wtc_branch.group_sys_admin" model="res.groups">
			<field name="implied_ids" eval="[(4,ref('group_settlement_cancel_read')),
				(4,ref('group_settlement_cancel_update')),
				(4,ref('group_settlement_cancel_delete')),
				
				]"/>
		</record>
		
    </data>
</openerp>