            <field name="implied_ids" eval="[(4, ref('group_teds_cash_count_read'))]"/>
        </record>     
        <record id="group_teds_cash_count_posted_button" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
