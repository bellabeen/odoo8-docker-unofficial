        <record id="wtc_branch.group_sys_admin" model="res.groups">
			<field name="implied_ids" eval="[
                (4,ref('group_teds_cash_count_generate_stock_button')),
                (4,ref('group_teds_cash_count_posted_button')),
                (4,ref('group_teds_cash_count_update_plafon_button')),
                (4,ref('group_teds_cash_count_approved_button')),
                (4,ref('group_teds_cash_count_cancel_button')),
