<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    	<record id="wtc_stock_production_permohonan_form_view" model="ir.ui.view">
            <field name="name">wtc.stock.production.permohonan.form.view</field>
            <field name="model">stock.production.lot</field>
            <field name="inherit_id" ref="stock.view_production_lot_form"/>
            <field name="arch" type="xml">
				<field name="invoice_date" position="after">
					<field name="invoice_bbn" readonly="1"/>
				</field>
	
				<field name ="tgl_terima" position="before">
					<field name="permohonan_faktur_id" readonly="1"/>
					<field name="penerimaan_faktur_id" readonly="1"/>
					<field name="penyerahan_faktur_id" readonly="1"/>					
				</field>
				<field name ="customer_id" position="before">
					<field name="cddb_id" readonly="1"/>	
					<field name="lot_status_cddb" readonly="1"/>			
				</field>
				<field name ="tgl_terima" position="after" readonly="1">
					<field name="tgl_cetak_faktur" readonly="1"/>	
					<field name="tgl_bayar_birojasa" readonly="1"></field>
					<field name="tgl_penyerahan_faktur" readonly="1"/>	
				</field>		
				<xpath expr='//notebook' position='after'>
				      <div class="oe_chatter">
	                    <field name="message_follower_ids" widget="mail_followers"/>
	                    <field name="message_ids" widget="mail_thread"/>
	                </div>
	             </xpath>
            </field>
        </record>
        
	</data>
</openerp>