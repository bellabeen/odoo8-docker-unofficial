<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data> 
    	
		<record model="ir.ui.view" id="wtc_cancel_permohonan_faktur_form_view">
		    <field name="name">wtc.cancel.permohonan.faktur.form</field>
		    <field name="model">wtc.cancel.permohonan.faktur</field>
		    <field name="arch" type="xml">
		        <form string="Cancel Permohonan Faktur" version="7.0">
					<header>
							<button name="wtc_cancel_permohonan_faktur" states='draft' groups="wtc_permohonan_faktur.group_wtc_cancel_permohonan_faktur_post" type="object" string="Post" class="oe_highlight"/>		
							<field name="state" widget="statusbar" statusbar_visible="draft,post" statusbar_colors='{"draft":"red","posy":"blue"}'/>	
	           		</header>	
	           		<sheet>	        	
				        	<div class="oe_title">
				        		<h2>
					                <label string="Cancel Permohonan Faktur STNK"/>    			
					 				<field name="name" class="oe_inline" readonly='1'/>
				 				</h2>
				        	</div>	
				        	<group>	
				        		<group>
				            		<field name="permohonan_faktur_id" required='1' options="{'no_create': True,'no_open': True}" attrs="{'readonly': [('state','!=','draft')]}"/>
		                        </group>
			            	</group>
			            	<notebook>
			            		<page string='Engine Line'>
				            	<field colspan="4" name="cancel_line" required='1' nolabel="1" widget="one2many_list" attrs="{'readonly': [('state','!=','draft')]}">
		                            <tree string="" editable="top">
						                <field name="name" options="{'no_create': True,'no_open': True}" on_change="onchange_engine(name)" string="Engine No"/>
										<field name="chassis_no" readonly='1'/>
		                            </tree>
		                        </field>			            		
			            		</page>
			            		<page string="Audit Trail">
									<group>
										<group>
											<field name="create_uid" readonly="1"></field>
											<field name="create_date" readonly="1"></field>
											<field name="confirm_uid" readonly="1"></field>
											<field name="confirm_date" readonly="1"></field>
										</group>
										<group>							
											<field name="write_uid" readonly="1"></field>
											<field name="write_date" readonly="1"></field>
										</group>
									</group>
								</page>
			            	</notebook>			            	
		            </sheet>	
		        </form>
		    </field>
		</record>
			
		<record model="ir.ui.view" id="cancel_permohonan_faktur_tree_view">
		    <field name="name">cancel.permohonan.faktur.tree</field>
		    <field name="model">wtc.cancel.permohonan.faktur</field>
		    <field name="arch" type="xml">
		        <tree string="List Cancel Permohonan Faktur">
		        	<field name="name"/>
		            <field name="permohonan_faktur_id"/>
					<field name="state"/>		            	            
		        </tree>
		     </field>
		</record>
				
		<record model="ir.actions.act_window" id="wtc_cancel_permohonan_faktur_wizard_action">
		    <field name="name">Cancel Permohonan Faktur</field>
		    <field name="res_model">wtc.cancel.permohonan.faktur</field>
		    <field name="view_type">form</field>
		    <field name="view_mode">tree,form</field>
		</record>
				    	
		<menuitem id="cancel_permohonan_faktur_submenu" name="Cancel Permohonan Faktur" parent="wtc_dealer_menu.stnk_bpkb_permohonan_faktur_menu"  action="wtc_cancel_permohonan_faktur_wizard_action" sequence="21"/> 				  
				
		<record model="ir.ui.view" id="view_wizard_eksport_cddb">
            <field name="name">view.wizard.eksport.cddb</field>
            <field name="model">eksport.cddb</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form col="4" string="Eksport CDDB Wizard" version="7.0">
                    <group>
                        <field name="type"/>
                        <field name="name" invisible="True"/>
                        <separator colspan="4"/>
                        <field name="data_file" nolabel="1" colspan="4" filename="name"/>
                    </group>
                    <footer>
                        <button name="export_file" string="Export" type="object" class="oe_highlight"/>
                        or 
                        <button string="Cancel" class="oe_link" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>
               
        <record model="ir.actions.act_window" id="action_export_cddb">
            <field name="name">Import Object</field>
            <field name="res_model">eksport.cddb</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
		<record model="ir.ui.view" id="permohonan_faktur_tree_view">
		    <field name="name">permohonan.faktur.tree</field>
		    <field name="model">wtc.permohonan.faktur</field>
		    <field name="arch" type="xml">
		        <tree string="List Permohonan Faktur">
		        	<field name="name"/>
		            <field name="branch_id"/>
					<field name="division"/>		            
		            <field name="partner_id"/>
					<field name="tgl_mohon_faktur"/>		            
		            <field name="state"/>	            
		        </tree>
		     </field>
		</record>
 
		<record model="ir.ui.view" id="permohonan_faktur_form_view">
		    <field name="name">permohonan.faktur.form</field>
		    <field name="model">wtc.permohonan.faktur</field>
		    <field name="arch" type="xml">
		        <form string="Form Permohonan Faktur">
				<header>
						<button name="post_permohonan" groups='wtc_permohonan_faktur.group_wtc_permohonan_faktur_post' type="object" string="Post" attrs="{'invisible': ['|',('exception_faktur','=',True),'|',('state','=','approved'),('state','=','cancel')]}" class="oe_highlight"/>		
						<button name="%(action_export_cddb)d" groups='wtc_permohonan_faktur.group_wtc_permohonan_faktur_export_file' type="action" states="approved" string="Export File" class="oe_highlight" />							
						<button name="cancel_permohonan" groups='wtc_permohonan_faktur.group_wtc_permohonan_faktur_cancel' type="object" states="approved,draft" string="Cancel" confirm="Apa anda yakin ingin membatalkan transaksi ini ?"/>		
						<field name="state" widget="statusbar" statusbar_visible="draft,approved" statusbar_colors='{"draft":"red","approved":"blue"}'/>	
           		</header>		        	
		        	<sheet>
			        	<div class="oe_title">
			        		<h2>
				                <label string="Permohonan Faktur STNK"/>    			
				 				<field name="name" class="oe_inline"/>
			 				</h2>
			        	</div>       
						<group col="2" colspan="2">   
							<group>
								<field name="branch_id" options="{'no_create': True,'no_open': True}" attrs="{'readonly': ['|',('state','!=','draft'),('name','!=',False)]}" on_change="onchange_branch_permohonan_faktur(branch_id,exception_faktur)"/>
							</group>            
						    <group>
	                    		<field name="division" select="1" required="1" attrs="{'readonly': [('state','!=','draft')]}"/>
	                    	</group>
	                    	<group>
							    <field name="partner_id" select="1"/>
							    <field name="ahm_code"/>
	                    	</group>
				            <group>  
								<field name="tgl_mohon_faktur" required="1" readonly="1"/>									               
								<field name="exception_faktur" on_change="(exception_faktur,serial_number_ids,branch_id)" attrs="{'readonly': [('state','!=','draft')]}"/>
							</group>   
						</group>
	                    <notebook colspan="5">
	                        <page string="Engine line">
	                            <field colspan="4" name="serial_number_ids" nolabel="1" widget="one2many_list" attrs="{'readonly': [('state','!=','draft')]}">
	                                <tree string="" editable="top" edit="false" create="false">
						                <field name="name" string="Engine No"/>
										<field name="chassis_no"/>						                
						                <field name="product_id"/>  
						                <field name="customer_id"/>																		                
						                <field name="customer_stnk"/>  
						                <field name="state"/>  
	                                </tree>
	                            </field>
	                        </page>
	                    </notebook>						
		           </sheet>
	                <div class="oe_chatter">
	                    <field name="message_follower_ids" widget="mail_followers"/>
	                    <field name="message_ids" widget="mail_thread"/>
	                </div>		           
		        </form>
		    </field>
		</record>
		
 		<record id="view_permohonan_faktur_filter" model="ir.ui.view">
            <field name="name">permohonan.faktur.list.select</field>
            <field name="model">wtc.permohonan.faktur</field>
            <field name="arch" type="xml">
                <search string="Search Permohonan Faktur">
                    <field name="name" string="Reference"/>
                    <field name="branch_id"/>     
                    <field name="engine_no"/> 
                    <field name="customer_stnk"/>                
                    <field name="partner_id"/>
                    <filter name="draft" string="Draft" domain="[('state','=','draft')]"/>
                    <filter name="post" string="Posted" domain="[('state','=','posted')]"/>
                    <group expand="0" string="Group By">
                        <filter string="Branch" icon="terp-partner" domain="[]" context="{'group_by':'branch_id'}"/>
                        <filter string="Tanggal" icon="terp-go-month" domain="[]" context="{'group_by':'tgl_mohon_faktur'}"/>                        	                   
                    </group>
                </search>
            </field>
        </record>

        <record id="view_cancel_permohonan_faktur_filter" model="ir.ui.view">
            <field name="name">cancel.permohonan.faktur.filter</field>
            <field name="model">wtc.cancel.permohonan.faktur</field>
            <field name="arch" type="xml">
                <search string="Search">
                    <field name="name"/>
                    <field name="permohonan_faktur_id"/>
                    <filter name="draft" string="Draft" domain="[('state','=','draft')]"/>
                    <filter name="post" string="Posted" domain="[('state','=','post')]"/>
                </search>
            </field>
        </record>
                       
		<record model="ir.actions.act_window" id="permohonan_faktur_action">
		    <field name="name">Permohonan Faktur STNK</field>
		    <field name="res_model">wtc.permohonan.faktur</field>
		    <field name="view_type">form</field>
		    <field name="view_mode">tree,form</field>
		    <field name="search_view_id" ref="view_permohonan_faktur_filter"/>
			<field name="context">{"search_default_draft":1}</field>		    
		</record>
					
		<menuitem id="permohonan_faktur_submenu" name="Permohonan Faktur" parent="wtc_dealer_menu.stnk_bpkb_permohonan_faktur_menu"  action="permohonan_faktur_action" sequence="20"/> 				  
	</data>
</openerp>
															