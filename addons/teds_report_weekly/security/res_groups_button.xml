<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">    
        <record id="group_report_weekly_generate" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Button Generate Report Weekly</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_report_weekly_konsolidate_read'))]"/>
        </record>     
        <record id="group_report_weekly_get_actual" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Button Act Actual Report Weekly</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_report_weekly_konsolidate_read'))]"/>
        </record>     
        <record id="group_report_weekly_confirm_periode" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Button Confirm Periode Report Weekly</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_report_weekly_konsolidate_read'))]"/>
        </record>     
        <record id="group_report_weekly_confirm" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Button Confirm Report Weekly</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_report_weekly_konsolidate_read'))]"/>
        </record>     
        <record id="group_report_weekly_set_to_draft" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Button Set to Draft Report Weekly</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_report_weekly_konsolidate_read'))]"/>
        </record>     

        <!-- SYS ADMIN -->
	 	<record id="wtc_branch.group_sys_admin" model="res.groups">
			<field name="implied_ids" eval="[
				(4,ref('group_report_weekly_generate')),
				(4,ref('group_report_weekly_get_actual')),
				(4,ref('group_report_weekly_confirm_periode')),
				(4,ref('group_report_weekly_confirm')),
                (4,ref('group_report_weekly_set_to_draft')),
			]"/> 
		</record>
	</data>
</openerp>