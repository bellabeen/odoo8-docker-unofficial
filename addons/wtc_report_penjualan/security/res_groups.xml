<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

<!-- Laporan Penjualan -->
		
        <record id="group_wtc_report_penjualan_wizard_read" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Laporan Penjualan Read</field>
            <field name="menu_access" eval="[
            								(4, ref('menu_penjualan_report')),
            								]"/>
        </record>

        <record id="group_wtc_report_penjualan_wizard_update" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Laporan Penjualan Update</field>
            <field name="menu_access" eval="[
            								(4, ref('menu_penjualan_report')),
            								]"/>
			<field name="implied_ids" eval="[(4, ref('group_wtc_report_penjualan_wizard_read'))]"/>
        </record>
        
        <record id="wtc_branch.group_sys_admin" model="res.groups">
			<field name="implied_ids" eval="[(4,ref('group_wtc_report_penjualan_wizard_read')),
											(4,ref('group_wtc_report_penjualan_wizard_update')),
											]"/>
		</record>

        <record id="group_wtc_report_penjualan_salesman_wizard_read" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Laporan Penjualan Salesman Read</field>
            <field name="menu_access" eval="[(4,ref('menu_penjualan_salesman_report')),]"/>
        </record>
    </data>
</openerp>
