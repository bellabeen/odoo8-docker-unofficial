<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">  	
    	
<!-- Laporan Payments -->	
	
        <record id="group_laporan_payment_read" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Laporan Payment</field> 
            <field name="menu_access" eval="[(4, ref('menu_report_payment'))]"/>  
        </record> 

        <record id="group_laporan_payment_update" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Laporan Payment Update</field> 
            <field name="menu_access" eval="[(4, ref('menu_report_payment'))]"/>   
			<field name="implied_ids" eval="[(4, ref('group_laporan_payment_read'))]"/>            	            	
        </record>
   
        <record id="wtc_branch.group_sys_admin" model="res.groups">
			<field name="implied_ids" eval="[(4,ref('group_laporan_payment_read')),
											(4,ref('group_laporan_payment_update')),

											]"/> 
		</record>                                                                                           
    </data>
</openerp>
