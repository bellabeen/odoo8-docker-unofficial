<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>
<template id="teds_disbursement_cancel_print_pdf">
    <t t-call="report.html_container">
        <t t-foreach="docs" t-as="o">
            <div class="page">
                <div class="oe_structure"/>
                    <div class="row"> 
                    </div>
                    <div class="col-xs-12" align="center">
                        <p><strong>DISBURSEMENT CANCEL</strong></p>
                    </div>                  
                    <div class="col-xs-8" style="font-size:10px">
                        <table class="table table-bordered">
                            <tr>
                                <th>Branch</th>
                                <th>:</th>
                                <td><span t-esc="data['branch_id']"/></td>
                            </tr>
                            <tr>
                                <th>Division</th>
                                <th>:</th>
                                <td><span t-esc="data['division']"/></td>
                            </tr>
                            <tr>
                                <th>Date</th>
                                <th>:</th>
                                <td><span t-esc="data['date']"/></td>
                            </tr>
                            <tr>
                                <th>Disbursement</th>
                                <th>:</th>
                                <td><span t-esc="data['disbursement']"/></td>
                            </tr>
                            <tr>
                                <th>Reason</th>
                                <th>:</th>
                                <td><span t-esc="data['reason']"/></td>
                            </tr>
                        </table>
                    </div>
                    <div  class="col-xs-12" style="font-size:10px">
                        <table class="table table-bordered">
                            <tr>
                                <td colspan="2" align="center">Diajukan Oleh</td>
                                <td align="center">Mengetahui</td>
                            </tr>
                            <tr>
                                <td height="50"></td>
                                <td height="50"></td>
                                <td height="50"></td>
                            </tr>
                            <tr>
                                <td height="20"></td>
                                <td height="20"></td>
                                <td height="20"></td>
                            </tr>
                            <tr>
                                <td align="center">Kasir</td>
                                <td align="center">ADH</td>
                                <td align="center">SOH</td>
                            </tr>
                        </table>
                        <p><span t-esc="data['today']"/>, <span t-esc="data['user']"/></p>
                    </div>

                <div class="oe_structure"/>
            </div>
        </t>
    </t>
</template>
</data>
</openerp>
