<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
	
	<!-- Stock Packing -->
	
	<record model="ir.ui.view" id="view_wizard_eksport_stnk_bpkb">
            <field name="name">view.wizard.eksport.stnk.bpkb</field>
            <field name="model">eksport.stnk.bpkb</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form col="4" string="Eksport STNK BPKB" version="7.0">
                    <group>
                        
                        <field name="name" invisible="True"/>
                        <group col="2" colspan="2">
	                    <group>
	                        <field name="date_start"/>
	                    </group>
	                	<group>
	                        <field name="date_end"/>
	                    </group>
                    </group>
                        <separator colspan="4"/>
                        <field name="data_file" nolabel="1" colspan="4" filename="name"/>
                    </group>
                    <footer>
                        <button name="export_file" string="Export" type="object" class="oe_highlight"/>
                        or 
                        <button string="Cancel" class="oe_link" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>
        
        <record model="ir.actions.act_window" id="action_export_stnk_bpkb">
            <field name="name">Generate STNK BPKB</field>
            <field name="res_model">eksport.stnk.bpkb</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

	<menuitem id="generate_stnk_bpkb_submenu"   parent="wtc_dealer_menu.stnk_bpkb_proses_stnk_menu"  sequence="100"  action="action_export_stnk_bpkb"/>
	        
	</data>
</openerp>