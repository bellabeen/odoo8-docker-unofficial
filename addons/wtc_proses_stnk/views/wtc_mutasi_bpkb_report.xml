<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>

<template id="wtc_mutasi_bpkb_report">
    <style type="text/css">
      @font-face {
            font-family: 'Agency FB';
            font-style: normal;
            font-weight: 400;
           <!--  src: local('Agency FB'), local('AgencyFB-Regular'),
                url(http://allfont.net/cache/fonts/agency-fb_0b9596562659a0772f5e24c22cae921b.woff) format('woff'),
                url(http://allfont.net/cache/fonts/agency-fb_0b9596562659a0772f5e24c22cae921b.ttf) format('truetype'); -->
        }

         h1   {font-size: 22; font-weight:bold; text-align: left; }
         h2   {font-size: 8; font-weight:bold; text-align: left; }
       


          table#t01 {text-align: left;width: 100%; border-left: 0px solid black; border-right: 0px solid black; border-bottom: 0px solid black; black; border-top: 1px solid black;}

         td#td01 {border-left: 1px solid black; border-right: 0px solid black; border-top: 0px solid black; border-bottom: 0px solid black; font-size: 12;} 
      
         tr#tr01 {font-size: 18; height: 10%; border-left: 1px solid black; border-right: 1px solid black; border-top: 0px solid black; border-bottom: 0px solid black; font-family: 'Agency FB';} 

         th#th01 {border-left: 1px solid black; border-right: 1px solid black; border-top: 1px solid black; border-bottom: 1px solid black; font-size: 12; font-family: 'Agency FB';}  

         tbody#tbody01 {border-left: 0px solid black; border-right: 0px solid black; border-top: 0px solid black; border-bottom: 1px solid black; font-size: 18; font-family: 'Agency FB';}
    </style>

  

    <t t-call="report.html_container">
        <t t-foreach="docs" t-as="o">
  
                 <div class="page" style="font-family: Agency FB;">
                    <div class="oe_structure"/>
                    <div class="row"></div>

                    <table width="100%" style="font-size: 16px; ">
                        <tr width="100.00%">
                            <td><h1><span t-field="o.branch_id.name"></span></h1></td>
                            <td style="font-size: 22px;">MUTASI BPKB</td>
                        </tr>
                        <tr width="30.00%">
                            <td style="font-size: 14px;"><span t-field="o.branch_id.name"></span></td>
                            <td>&#160;</td>
                        </tr>
                        <h1>&#160;</h1>
                        <tr width="50.00%">
                            <td width="35.00%" style="font-size: 14px;"><span t-field="o.branch_id.street"></span>-<span id="s01" t-field="o.branch_id.state_id.name"></span></td>
                            <td width="15.00%" style="font-size: 14px;">No : <span t-field="o.name"></span></td>
                        </tr>
                    </table>
                    <h2>&#160;</h2>

                    <table width="100%" style="border-collapse: collapse; border: 1px solid black; font-size: 16px;" cellpadding="5">
                        <tr>
                            <td width="18.00%">&#160; Tujuan</td>
                            <td>:</td>
                            <td><span id="s01" t-field="o.branch_id.name"></span></td>
                        </tr>
                        <tr>
                            <td width="18.00%">&#160; Tanggal Mutasi</td>
                            <td>:</td>
                            <td><span id="s01" t-field="o.tgl_mutasi"></span></td>
                        </tr>
                        <tr>
                            <td width="18.00%">&#160; Lokasi Asal</td>
                            <td>:</td>
                            <td><span id="s01" t-field="o.source_location_id"></span></td>

                        </tr>
                        <tr>
                            <td width="18.00%">&#160; Lokasi Tujuan</td>
                            <td>:</td>
                            <td><span id="s01" t-field="o.destination_location_id"></span></td>
                        </tr>
                        <tr>
                            <td width="18.00%">&#160;</td>
                            <td>&#160;</td>
                            <td>&#160;</td>
                        </tr>
                    </table>

                    <table id="t01" style="font-size: 10px;">
                          <tr style="border-bottom: 1px solid black; font-size: 8px;">
                          <th id="th01" height="25" width="5.00%"> &#160; No              </th>
                          <th id="th01" height="25" width="13.00%">&#160; No Mesin        </th>
                          <th id="th01" height="25" width="15.00%">&#160; No Rangka       </th>
                          <th id="th01" height="25" width="21.00%">&#160; Atas Nama       </th>
                          <th id="th01" height="25" width="12.00%">&#160; No BPKB         </th>
                          <th id="th01" height="25" width="15.00%">&#160; Tgl Jadi BPKB   </th>
                          <th id="th01" height="25" width="24.00%">&#160; Tgl Terima BPKB </th>
                          <th id="th01" height="25" width="10.00%">&#160; No Urut         </th>
                          </tr>

                         <tbody id="tbody01" style="font-size: 10px;">
                          <tr id="tr01" t-foreach="o.mutasi_line" t-as="k">
                          <td id="td01" style="text-align: Center" height="25"><span t-esc="no_urut()"/></td>
                          <td id="td01" height="25">&#160;<span t-field="k.name.name"/></td>
                          <td id="td01" height="25">&#160;<span t-field="k.name.chassis_no"/></td>
                          <td id="td01" height="25">&#160;<span t-field="k.customer_stnk.name"/></td>
                          <td id="td01" height="25">&#160;<span t-field="k.no_bpkb"/></td>
                          <td id="td01" height="25">&#160;<span t-field="k.tgl_bpkb"/></td>
                          <td id="td01" height="25">&#160;<span t-field="k.tgl_terima_bpkb"/></td>
                          <td id="td01" height="25">&#160;<span t-field="k.no_urut_bpkb"/></td> 
                          </tr>
                          </tbody>
                    </table>
                   
                   <div class="oe_structure" style="font-family: Agency FB;"> 
                   <table style="width: 100%; font-size: 16px;">
                            <tr>
                                <td width="5.00%"><h1></h1></td>
                                <td style="font-size: 16px;">Dicetak Oleh</td>
                                <td width="20.00%"><h1></h1></td>
                                <td style="font-size: 16px;">Penerima</td>
                            </tr> 
                            <tr>
                                <td width="5.00%"><h1></h1></td>
                                <td><h1></h1></td>
                                <td><h1></h1></td>
                                <td><h1></h1></td>
                            </tr>  
                            <tr>
                                <td width="5.00%"><h1></h1></td>
                                <td><h1></h1></td>
                                <td><h1></h1></td>
                                <td><h1></h1></td>
                            </tr>
                            <tr>
                                <td width="5.00%"><h1></h1></td>
                                <td>...................................................</td>
                                <td width="20.00%"><h1></h1></td>
                                <td>...................................................</td>
                            </tr> 
                            <tr>
                                <td><h1></h1></td>
                                <td style="font-size: 16px;"> <span t-esc="usr()"/>,<span t-esc="tgl()"/></td>
                                <td width="20.00%"><h1></h1></td>
                            </tr>                                            
                            <tr>
                                <td width="5.00%"><h1></h1></td>
                                <td style="font-size: 16px;">Lembar ke-1 Pengirim / Lembar ke-2 Penerima</td>
                            </tr> 
                        </table> 
                    </div>
                </div>
        </t>
    </t>
</template>
</data>
</openerp>