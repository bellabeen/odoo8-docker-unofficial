<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
		<record id="view_teds_b2b_api_schedule_tree" model="ir.ui.view">
            <field name="name">teds.b2b.api.schedule.tree</field>
            <field name="model">teds.b2b.api.schedule</field>
            <field name="arch" type="xml">
                <tree string="Api Schedule">
                    <field name="name"/>
                </tree>
            </field>
        </record>
        
        <record id="view_teds_b2b_api_schedule_form" model="ir.ui.view">
            <field name="name">teds.b2b.api.schedule.form</field>
            <field name="model">teds.b2b.api.schedule</field>
            <field name="arch" type="xml">
                <form string="Api Schedule">
                    <sheet>
                        <group>                          
                            <field name="name" required="1"/>
                        </group>
                        <notebook>
                            <page string="Detail">
                                <field name="detail_ids">
                                    <tree editable="top">
                                        <field name="jam" required="1"/>
                                        <field name="menit" required="1"/>
                                    </tree>
                                </field>
                            </page>
                            <page string="Audit Trail">
                                <group>
                                    <group>
                                        <field name="create_uid" options="{'no_open':True,'no_create':True}" readonly="1"></field>
                                        <field name="create_date" readonly="1"></field>
                                    </group>
                                    <group>                         
                                        <field name="write_uid" options="{'no_open':True,'no_create':True}" readonly="1"></field>
                                        <field name="write_date" readonly="1"></field>
                                    </group>
                                </group>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="action_teds_b2b_api_schedule" model="ir.actions.act_window">
            <field name="name">B2b API Schedule</field>
            <field name="res_model">teds.b2b.api.schedule</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        
        <menuitem id="teds_b2b_api_menu" name="B2B API" sequence="70"/>
        
        <menuitem id="teds_b2b_api_top_menu" name="B2B" sequence="1" parent="teds_b2b_api_menu"/>
        
        <menuitem id="teds_b2b_api_top_configuration_menu" name="B2B Configuration" parent="teds_b2b_api_menu" sequence="2"/>

        <menuitem id="teds_b2b_api_schedule_menuitem" parent="teds_b2b_api_top_menu" action="action_teds_b2b_api_schedule" sequence="1"/>

    </data>
</openerp>