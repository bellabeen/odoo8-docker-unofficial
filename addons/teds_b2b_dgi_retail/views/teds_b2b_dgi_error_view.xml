<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
		<record id="view_teds_b2b_dgi_error_log_tree" model="ir.ui.view">
            <field name="name">teds.b2b.dgi.error.log.tree</field>
            <field name="model">teds.b2b.dgi.error.log</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="url"/>
                    <field name="date"/>
                    <field name="request_type"/>
                    <field name="error"/>
                </tree>
            </field>
        </record>
        
        <record id="view_teds_b2b_dgi_error_log_form" model="ir.ui.view">
            <field name="name">teds.b2b.dgi.error.log.form</field>
            <field name="model">teds.b2b.dgi.error.log</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="origin"/>
                            <field name="url"/>
                            <field name="date"/>
                            <field name="request_type"/>
                            <field name="error"/> 
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="action_teds_b2b_dgi_error_log" model="ir.actions.act_window">
            <field name="name">DGI Errors</field>
            <field name="res_model">teds.b2b.dgi.error.log</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        
        <menuitem id="teds_b2b_dgi_error_menuitem" action="action_teds_b2b_dgi_error_log" parent="teds_b2b_api_configuration.teds_b2b_api_top_configuration_menu" sequence="1"/>
    </data>
</openerp>