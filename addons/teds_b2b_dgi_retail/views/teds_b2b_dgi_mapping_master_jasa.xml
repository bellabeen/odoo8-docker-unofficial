<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
		<record id="view_teds_b2b_dgi_mapping_master_jasa_tree" model="ir.ui.view">
            <field name="name">teds.b2b.dgi.mapping.master.jasa.tree</field>
            <field name="model">teds.b2b.dgi.mapping.master.jasa</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="md_id"/>
                    <field name="branch_id"/>
                    <field name="product_id"/>
                    <field name="value"/>
                    <field name="sequence"/>
                </tree>
            </field>
        </record>
        
        <record id="view_teds_b2b_dgi_mapping_master_jasa_form" model="ir.ui.view">
            <field name="name">teds.b2b.dgi.mapping.master.jasa.form</field>
            <field name="model">teds.b2b.dgi.mapping.master.jasa</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="md_id" options="{'no_open':True,'no_create':True}" required="1"/>
                            <field name="branch_id" options="{'no_open':True,'no_create':True}"/>
                            <field name="product_id" options="{'no_open':True,'no_create':True}" required="1"/>
                            <field name="value" required="1"/>
                            <field name="sequence" required="1"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="action_teds_b2b_dgi_mapping_master_jasa" model="ir.actions.act_window">
            <field name="name">Mapping Master Jasa</field>
            <field name="res_model">teds.b2b.dgi.mapping.master.jasa</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="view_teds_b2b_dgi_mapping_master_jasa_search" model="ir.ui.view">
            <field name="name">teds.b2b.dgi.mapping.master.jasa.search</field>
            <field name="model">teds.b2b.dgi.mapping.master.jasa</field>
            <field name="arch" type="xml">
                <search string="Search">
                    <field name="product_id"/>
                    <field name="value"/>
                    <field name="md_id"/>
                    <field name="branch_id"/>
                    <group expand="0" string="Group By...">
                        <filter name="group_by_md_id" string="Main Dealer" domain="[]" context="{'group_by':'md_id'}"/>
                        <filter name="group_by_branch_id" string="Branch" domain="[]" context="{'group_by':'branch_id'}"/>
                        <filter name="group_by_product_id" string="Jasa" domain="[]" context="{'group_by':'product_id'}"/>
                        <filter name="group_by_value" string="Kode MD" domain="[]" context="{'group_by':'value'}"/>
                    </group>
                </search>
            </field>
        </record>

        
        <menuitem id="teds_b2b_dgi_mapping_master_jasa_menuitem" action="action_teds_b2b_dgi_mapping_master_jasa" parent="teds_b2b_api_configuration.teds_b2b_api_top_configuration_menu" sequence="2"/>
    </data>
</openerp>