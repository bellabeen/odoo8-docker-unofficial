<?xml version="1.0"?>
<openerp>
	<data>
	  	<record id="view_teds_employee_code_md_form" model="ir.ui.view" >
		    <field name="name">teds.employee.code.md.form</field>
		    <field name="model">hr.employee</field>
		    <field name="inherit_id" ref="wtc_hr_employee.wtc_employee_form3_view"/>
		    <field name="arch" type="xml">
		    	<field name="sales_ahm" position="after">
		    		<field name="code_md"/>
		    	</field>
 		    </field>
		</record>

		<record id="view_teds_employee_dgi_filter" model="ir.ui.view">
	        <field name="name">teds.employe.dgi.filter</field>
	        <field name="model">hr.employee</field>
	        <field name="inherit_id" ref="hr.view_employee_filter"/>
	        <field name="arch" type="xml">
            	<field name="category_ids" position="before">
            		<field name="branch_id"/>
            		<field name="code_md" string="ID Main Dealer"/>
            	</field>
            </field>
	    </record>
	</data>
</openerp>