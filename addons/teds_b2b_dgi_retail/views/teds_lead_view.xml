<?xml version="1.0"?>
<openerp>
	<data>
	  	<record id="view_teds_lead_log_note_dgi_form" model="ir.ui.view" >
		    <field name="name">teds.lead.log.note.dgi.form</field>
		    <field name="model">teds.lead</field>
		    <field name="inherit_id" ref="teds_lead.view_teds_lead_form"/>
		    <field name="arch" type="xml">
				<field name="customer_id" position="after">
					<field name="md_reference_prospect" readonly="1"/>
					<field name="md_reference_spk" readonly="1"/>
					
				</field>
				<xpath expr="//notebook" position="inside">
 					<page string="Tracking DGI Log" attrs="{'invisible':[('note_log','=',False)]}">
 						<field name="note_log" readonly="1"/>
 					</page>
 				</xpath>
		    </field>
		</record>
	</data>
</openerp>