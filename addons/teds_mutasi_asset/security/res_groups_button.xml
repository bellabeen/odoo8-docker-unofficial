<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
<!-- Mutasi Request Form -->
		<!-- Approved Mutasi Request -->
        <record id="group_teds_mutasi_request_asset_approved" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Mutation Request Asset Approved</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_mutation_request_asset_read'))]"/>
        </record>
        
        <!-- Cancel Approve RFA Mutasi Request -->
        <record id="group_teds_mutasi_request_asset_cancel_approve_rfa" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Mutation Request Asset Cancel Approve RFA</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_mutation_request_asset_read'))]"/>
        </record>
		
		<!-- Cancel Approve  Mutasi Request -->
        <record id="group_teds_mutasi_request_asset_cancel_approve" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Mutation Request Asset Cancel Approve</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_mutation_request_asset_read'))]"/>
        </record>
        
        <!-- Confirm Mutasi Request -->
        <record id="group_teds_mutasi_request_asset_confirm" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Mutation Request Asset Confirm</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_mutation_request_asset_read'))]"/>
        </record>

<!-- Mutasi Asset Form -->
		<!-- Approved Mutasi Asset -->
        <record id="group_teds_mutasi_asset_apprved" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Mutation Asset Approved</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_mutation_asset_read'))]"/>
        </record>
        
        <!-- Cancel Approve RFA Mutasi Request -->
        <record id="group_teds_mutasi_asset_cancel_approve_rfa" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Mutation Asset Cancel Approve RFA</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_mutation_asset_read'))]"/>
        </record>
		
		<!-- Cancel Approve Mutasi Request -->
        <record id="group_teds_mutasi_asset_cancel_approve" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Mutation Asset Cancel Approve</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_mutation_asset_read'))]"/>
        </record>
        
        <!-- Confirm Mutasi Request -->
        <record id="group_teds_mutasi_asset_confirm" model="res.groups">
            <field name="category_id" ref="wtc_branch.tdm_role"/>
            <field name="name">Mutation Asset Confirm</field>
            <field name="implied_ids" eval="[(4, ref('group_teds_mutation_asset_read'))]"/>
        </record>

<!-- Sistem  Admin -->
        <record id="wtc_branch.group_sys_admin" model="res.groups">
            <field name="implied_ids" eval="[
                (4,ref('group_teds_mutasi_request_asset_approved')),
                (4,ref('group_teds_mutasi_request_asset_cancel_approve_rfa')),
                (4,ref('group_teds_mutasi_request_asset_cancel_approve')),
                (4,ref('group_teds_mutasi_request_asset_confirm')),
                (4,ref('group_teds_mutasi_asset_apprved')),
                (4,ref('group_teds_mutasi_asset_cancel_approve_rfa')),
                (4,ref('group_teds_mutasi_asset_cancel_approve')),
                (4,ref('group_teds_mutasi_asset_confirm')),
            ]"/> 
            </record> 

    </data>
</openerp>